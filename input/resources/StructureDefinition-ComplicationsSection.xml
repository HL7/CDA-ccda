    </identifier>
    <version value="2015-08-01"/>
    <name value="ComplicationsSection"/>
    <title value="Complications Section"/>
    <status value="active"/>
    <description value="This section contains problems that occurred during or around the time of a procedure. The complications may be known risks or unanticipated problems."/>
    <fhirVersion value="4.0.1"/>
    <kind value="logical"/>
    <abstract value="false"/>
    <type value="http://hl7.org/cda/stds/core/StructureDefinition/Section"/>
    <baseDefinition value="http://hl7.org/cda/stds/core/StructureDefinition/Section"/>
    <derivation value="constraint"/>
    <differential>
        <element id="Section">
            <path value="Section"/>
        </element>
        <element id="Section.templateId">
            <path value="Section.templateId"/>
            <slicing>
                <discriminator>
                    <type value="value"/>
                    <path value="root"/>
                </discriminator>
                <discriminator>
                    <type value="value"/>
                    <path value="extension"/>
                </discriminator>
                <rules value="open"/>
            </slicing>
            <min value="1"/>
        </element>
        <element id="Section.templateId:primary">
            <path value="Section.templateId"/>
            <sliceName value="primary"/>
            <comment value="SHALL contain exactly one [1..1] templateId (CONF:1198-8174) such that it"/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Section.templateId:primary.root">
            <path value="Section.templateId.root"/>
            <comment value="SHALL contain exactly one [1..1] @root=&quot;2.16.840.1.113883.10.20.22.2.37&quot; (CONF:1198-10384)."/>
