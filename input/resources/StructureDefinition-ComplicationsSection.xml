            <max value="1"/>
            <patternString value="2.16.840.1.113883.10.20.22.2.37"/>
        </element>
        <element id="Section.templateId:primary.extension">
            <path value="Section.templateId.extension"/>
            <comment value="SHALL contain exactly one [1..1] @extension=&quot;2015-08-01&quot; (CONF:1198-32538)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="2015-08-01"/>
        </element>
        <element id="Section.code">
            <path value="Section.code"/>
            <comment value="SHALL contain exactly one [1..1] code (CONF:1198-15453)."/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Section.code.code">
            <path value="Section.code.code"/>
            <comment value="This code SHALL contain exactly one [1..1] @code=&quot;55109-3&quot; Complications (CONF:1198-15454)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="55109-3"/>
        </element>
        <element id="Section.code.codeSystem">
            <path value="Section.code.codeSystem"/>
            <comment value="This code SHALL contain exactly one [1..1] @codeSystem=&quot;2.16.840.1.113883.6.1&quot; (CodeSystem: LOINC urn:oid:2.16.840.1.113883.6.1) (CONF:1198-30860)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="2.16.840.1.113883.6.1"/>
        </element>
        <element id="Section.title">
            <path value="Section.title"/>
            <comment value="SHALL contain exactly one [1..1] title (CONF:1198-8176)."/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Section.text">
            <path value="Section.text"/>
            <comment value="SHALL contain exactly one [1..1] text (CONF:1198-8177)."/>
            <min value="1"/>
            <max value="1"/>
        </element>
