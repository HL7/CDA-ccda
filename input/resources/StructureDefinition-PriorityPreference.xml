<?xml version="1.0" encoding="UTF-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
    <id value="PriorityPreference"/>
    <extension url="http://hl7.org/fhir/tools/StructureDefinition/logical-target">
        <valueBoolean>
            <extension url="http://hl7.org/fhir/StructureDefinition/data-absent-reason">
                <valueCode value="not-applicable"/>
            </extension>
        </valueBoolean>
    </extension>
    <extension url="http://hl7.org/fhir/StructureDefinition/elementdefinition-namespace">
        <valueUri value="urn:hl7-org:v3"/>
    </extension>
    <extension url="http://hl7.org/fhir/StructureDefinition/elementdefinition-xml-name">
        <valueUri value="observation"/>
    </extension>
    <extension url="http://hl7.org/fhir/tools/StructureDefinition/type-profile-style">
        <valueUri value="cda"/>
    </extension>
    <url value="http://hl7.org/cda/us/ccda/StructureDefinition/PriorityPreference"/>
    <identifier>
        <value value="urn:oid:2.16.840.1.113883.10.20.22.4.143"/>
    </identifier>
    <name value="PriorityPreference"/>
    <title value="Priority Preference"/>
    <status value="draft"/>
    <description value="This template represents priority preferences chosen by a patient or a care provider. Priority preferences are choices made by care providers or patients or both relative to options for care or treatment (including scheduling, care experience, and meeting of personal health goals), the sharing and disclosure of health information, and the prioritization of concerns and problems."/>
    <fhirVersion value="5.0.0"/>
    <mapping>
        <identity value="rim"/>
        <uri value="http://hl7.org/v3"/>
        <name value="RIM Mapping"/>
    </mapping>
    <kind value="logical"/>
    <abstract value="false"/>
    <type value="http://hl7.org/cda/stds/core/StructureDefinition/Observation"/>
    <baseDefinition value="http://hl7.org/cda/stds/core/StructureDefinition/Observation"/>
    <derivation value="constraint"/>
    <differential>
        <element id="Observation">
            <path value="Observation"/>
            <constraint>
                <key value="should-text-ref-value"/>
                <severity value="warning"/>
                <human value="SHOULD contain text/reference/@value"/>
                <expression value="text.reference.value.exists()"/>
                <source value="http://hl7.org/cda/us/ccda/StructureDefinition/PriorityPreference"/>
            </constraint>
            <constraint>
                <key value="should-effectiveTime"/>
                <severity value="warning"/>
                <human value="SHOULD contain effectiveTime"/>
                <expression value="effectiveTime.exists()"/>
                <source value="http://hl7.org/cda/us/ccda/StructureDefinition/PriorityPreference"/>
            </constraint>
            <constraint>
                <key value="should-author"/>
                <severity value="warning"/>
                <human value="SHOULD contain author"/>
                <expression value="author.exists()"/>
                <source value="http://hl7.org/cda/us/ccda/StructureDefinition/PriorityPreference"/>
            </constraint>
        </element>
        <element id="Observation.templateId">
            <path value="Observation.templateId"/>
            <slicing>
                <discriminator>
                    <type value="value"/>
                    <path value="root"/>
                </discriminator>
                <rules value="open"/>
            </slicing>
            <min value="1"/>
        </element>
        <element id="Observation.templateId:priority-pref">
            <path value="Observation.templateId"/>
            <sliceName value="priority-pref"/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Observation.templateId:priority-pref.root">
            <path value="Observation.templateId.root"/>
            <min value="1"/>
            <patternString value="2.16.840.1.113883.10.20.22.4.143"/>
        </element>
        <element id="Observation.templateId:priority-pref.extension">
            <path value="Observation.templateId.extension"/>
            <max value="0"/>
        </element>
        <element id="Observation.classCode">
            <path value="Observation.classCode"/>
            <comment value="SHALL contain exactly one [1..1] @classCode=&quot;OBS&quot; (CodeSystem: HL7ActClass urn:oid:2.16.840.1.113883.5.6) (CONF:1098-30949)."/>
            <fixedCode value="OBS"/>
        </element>
        <element id="Observation.moodCode">
            <path value="Observation.moodCode"/>
            <comment value="SHALL contain exactly one [1..1] @moodCode=&quot;EVN&quot; (CodeSystem: HL7ActMood urn:oid:2.16.840.1.113883.5.1001) (CONF:1098-30950)."/>
            <fixedCode value="EVN"/>
        </element>
        <element id="Observation.id">
            <path value="Observation.id"/>
            <comment value="SHALL contain at least one [1..*] id (CONF:1098-30953)."/>
            <min value="1"/>
        </element>
        <element id="Observation.code">
            <path value="Observation.code"/>
            <comment value="SHALL contain exactly one [1..1] code (CONF:1098-30954)."/>
        </element>
        <element id="Observation.code.code">
            <path value="Observation.code.code"/>
            <comment value="This code SHALL contain exactly one [1..1] @code=&quot;225773000&quot; Preference (CONF:1098-30955)."/>
            <min value="1"/>
            <patternCode value="225773000"/>
        </element>
        <element id="Observation.code.codeSystem">
            <path value="Observation.code.codeSystem"/>
            <comment value="This code SHALL contain exactly one [1..1] @codeSystem=&quot;2.16.840.1.113883.6.96&quot; (CodeSystem: SNOMED CT urn:oid:2.16.840.1.113883.6.96) (CONF:1098-30956)."/>
            <min value="1"/>
            <patternString value="2.16.840.1.113883.6.96"/>
        </element>
        <element id="Observation.text">
            <path value="Observation.text"/>
            <short value="SHOULD reference the portion of section narrative text corresponding to this entry"/>
        </element>
        <element id="Observation.text.reference">
            <path value="Observation.text.reference"/>
            <constraint>
                <key value="value-starts-octothorpe"/>
                <severity value="error"/>
                <human value="If reference/@value is present, it SHALL begin with a '#' and SHALL point to its corresponding narrative"/>
                <expression value="value.exists() implies value.startsWith('#')"/>
                <source value="http://hl7.org/cda/us/ccda/StructureDefinition/PriorityPreference"/>
            </constraint>
        </element>
        <element id="Observation.effectiveTime">
            <path value="Observation.effectiveTime"/>
            <comment value="SHOULD contain zero or one [0..1] effectiveTime (CONF:1098-32327)."/>
        </element>
        <element id="Observation.value">
            <path value="Observation.value"/>
            <comment value="SHALL contain exactly one [1..1] value with @xsi:type=&quot;CD&quot;, where the code SHALL be selected from ValueSet Priority Level urn:oid:2.16.840.1.113883.11.20.9.60 DYNAMIC (CONF:1098-30957)."/>
            <min value="1"/>
            <max value="1"/>
            <type>
                <code value="http://hl7.org/cda/stds/core/StructureDefinition/CD"/>
            </type>
            <binding>
                <strength value="required"/>
                <valueSet value="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.11.20.9.60"/>
            </binding>
        </element>
        <element id="Observation.author">
            <path value="Observation.author"/>
            <comment value="SHOULD contain zero or more [0..*] Author Participation (identifier: urn:oid:2.16.840.1.113883.10.20.22.4.119) (CONF:1098-30958)."/>
            <type>
                <code value="http://hl7.org/cda/stds/core/StructureDefinition/Author"/>
                <profile value="http://hl7.org/cda/us/ccda/StructureDefinition/AuthorParticipation"/>
            </type>
        </element>
    </differential>
</StructureDefinition>