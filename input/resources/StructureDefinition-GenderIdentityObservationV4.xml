        <element id="Observation.templateId:templateId1.root">
            <path value="Observation.templateId.root"/>
            <short value="root"/>
            <definition value="SHALL contain exactly one [1..1] @root=&quot;2.16.840.1.113883.10.20.34.3.45&quot; (CONF:4537-1225)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="2.16.840.1.113883.10.20.34.3.45"/>
        </element>
        <element id="Observation.templateId:templateId1.extension">
            <path value="Observation.templateId.extension"/>
            <short value="extension"/>
            <definition value="SHALL contain exactly one [1..1] @extension=&quot;2023-05-01&quot; (CONF:4537-1226)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="2023-05-01"/>
        </element>
        <element id="Observation.code">
            <path value="Observation.code"/>
            <short value="code"/>
            <definition value="SHALL contain exactly one [1..1] code (CONF:4537-1222)."/>
            <min value="1"/>
