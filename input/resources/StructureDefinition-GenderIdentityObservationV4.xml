        <element id="Observation.code.codeSystem">
            <path value="Observation.code.codeSystem"/>
            <short value="codeSystem"/>
            <definition value="This code SHALL contain exactly one [1..1] @codeSystem=&quot;2.16.840.1.113883.6.1&quot; (CodeSystem: LOINC urn:oid:2.16.840.1.113883.6.1 STATIC) (CONF:4537-1228)."/>
