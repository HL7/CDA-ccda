<?xml version="1.0" encoding="UTF-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
    <id value="HealthStatusObservation"/>
    <extension url="http://hl7.org/fhir/tools/StructureDefinition/logical-target">
        <valueBoolean>
            <extension url="http://hl7.org/fhir/StructureDefinition/data-absent-reason">
                <valueCode value="not-applicable"/>
            </extension>
        </valueBoolean>
    </extension>
    <extension url="http://hl7.org/fhir/tools/StructureDefinition/xml-namespace">
        <valueUri value="urn:hl7-org:v3"/>
    </extension>
    <extension url="http://hl7.org/fhir/tools/StructureDefinition/xml-name">
        <valueString value="observation"/>
    </extension>
    <extension url="http://hl7.org/fhir/tools/StructureDefinition/type-profile-style">
        <valueUri value="cda"/>
    </extension>
    <url value="http://hl7.org/cda/us/ccda/StructureDefinition/HealthStatusObservation"/>
    <identifier>
        <value value="urn:hl7ii:2.16.840.1.113883.10.20.22.4.5:2014-06-09"/>
    </identifier>
    <version value="2014-06-09"/>
    <name value="HealthStatusObservation"/>
    <title value="Health Status Observation"/>
    <status value="draft"/>
    <description value="This template represents information about the overall health status of the patient. To represent the impact of a specific problem or concern related to the patient's expected health outcome use the Prognosis Observation template 2.16.840.1.113883.10.20.22.4.113."/>
    <fhirVersion value="5.0.0"/>
    <mapping>
        <identity value="rim"/>
        <uri value="http://hl7.org/v3"/>
        <name value="RIM Mapping"/>
    </mapping>
    <kind value="logical"/>
    <abstract value="false"/>
    <type value="http://hl7.org/cda/stds/core/StructureDefinition/Observation"/>
    <baseDefinition value="http://hl7.org/cda/stds/core/StructureDefinition/Observation"/>
    <derivation value="constraint"/>
    <differential>
        <element id="Observation">
            <path value="Observation"/>
            <constraint>
                <key value="should-text-ref-value"/>
                <severity value="warning"/>
                <human value="SHOULD contain text/reference/@value"/>
                <expression value="text.reference.value.exists()"/>
                <source value="http://hl7.org/cda/us/ccda/StructureDefinition/HealthStatusObservation"/>
            </constraint>
        </element>
        <element id="Observation.templateId">
            <path value="Observation.templateId"/>
            <slicing>
                <discriminator>
                    <type value="value"/>
                    <path value="root"/>
                </discriminator>
                <discriminator>
                    <type value="value"/>
                    <path value="extension"/>
                </discriminator>
                <rules value="open"/>
            </slicing>
            <min value="1"/>
        </element>
        <element id="Observation.templateId:health-status-obs">
            <path value="Observation.templateId"/>
            <sliceName value="health-status-obs"/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Observation.templateId:health-status-obs.root">
            <path value="Observation.templateId.root"/>
            <min value="1"/>
            <patternString value="2.16.840.1.113883.10.20.22.4.5"/>
        </element>
        <element id="Observation.templateId:health-status-obs.extension">
            <path value="Observation.templateId.extension"/>
            <min value="1"/>
            <patternString value="2014-06-09"/>
        </element>
        <element id="Observation.classCode">
            <path value="Observation.classCode"/>
            <comment value="SHALL contain exactly one [1..1] @classCode=&quot;OBS&quot; Observation (CodeSystem: HL7ActClass urn:oid:2.16.840.1.113883.5.6 STATIC) (CONF:1098-9057)."/>
            <fixedCode value="OBS"/>
        </element>
        <element id="Observation.moodCode">
            <path value="Observation.moodCode"/>
            <comment value="SHALL contain exactly one [1..1] @moodCode=&quot;EVN&quot; Event (CodeSystem: HL7ActMood urn:oid:2.16.840.1.113883.5.1001 STATIC) (CONF:1098-9072)."/>
            <fixedCode value="EVN"/>
        </element>
        <element id="Observation.id">
            <path value="Observation.id"/>
            <comment value="SHALL contain at least one [1..*] id (CONF:1098-32486)."/>
            <min value="1"/>
        </element>
        <element id="Observation.code">
            <path value="Observation.code"/>
            <comment value="SHALL contain exactly one [1..1] code (CONF:1098-19143)."/>
        </element>
        <element id="Observation.code.code">
            <path value="Observation.code.code"/>
            <comment value="This code SHALL contain exactly one [1..1] @code=&quot;11323-3&quot; Health status (CONF:1098-19144)."/>
            <min value="1"/>
            <patternCode value="11323-3"/>
        </element>
        <element id="Observation.code.codeSystem">
            <path value="Observation.code.codeSystem"/>
            <comment value="This code SHALL contain exactly one [1..1] @codeSystem=&quot;2.16.840.1.113883.6.1&quot; (CodeSystem: LOINC urn:oid:2.16.840.1.113883.6.1) (CONF:1098-32161)."/>
            <min value="1"/>
            <patternString value="2.16.840.1.113883.6.1"/>
        </element>
        <element id="Observation.text">
            <path value="Observation.text"/>
            <short value="SHOULD reference the portion of section narrative text corresponding to this entry"/>
        </element>
        <element id="Observation.text.reference">
            <path value="Observation.text.reference"/>
            <constraint>
                <key value="value-starts-octothorpe"/>
                <severity value="error"/>
                <human value="If reference/@value is present, it SHALL begin with a '#' and SHALL point to its corresponding narrative"/>
                <expression value="value.exists() implies value.startsWith('#')"/>
                <source value="http://hl7.org/cda/us/ccda/StructureDefinition/HealthStatusObservation"/>
            </constraint>
        </element>
        <element id="Observation.statusCode">
            <path value="Observation.statusCode"/>
            <comment value="SHALL contain exactly one [1..1] statusCode (CONF:1098-9074)."/>
            <min value="1"/>
        </element>
        <element id="Observation.statusCode.code">
            <path value="Observation.statusCode.code"/>
            <comment value="This statusCode SHALL contain exactly one [1..1] @code=&quot;completed&quot; Completed (CodeSystem: HL7ActStatus urn:oid:2.16.840.1.113883.5.14 STATIC) (CONF:1098-19103)."/>
            <min value="1"/>
            <fixedCode value="completed"/>
        </element>
        <element id="Observation.value">
            <path value="Observation.value"/>
            <comment value="SHALL contain exactly one [1..1] value with @xsi:type=&quot;CD&quot;, where the code SHALL be selected from ValueSet HealthStatus urn:oid:2.16.840.1.113883.1.11.20.12 DYNAMIC (CONF:1098-9075)."/>
            <min value="1"/>
            <max value="1"/>
            <type>
                <code value="http://hl7.org/cda/stds/core/StructureDefinition/CD"/>
            </type>
            <binding>
                <strength value="required"/>
                <valueSet value="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.1.11.20.12"/>
            </binding>
        </element>
    </differential>
</StructureDefinition>