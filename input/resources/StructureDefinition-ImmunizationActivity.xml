        <element id="SubstanceAdministration.entryRelationship:instruction.act">
            <path value="SubstanceAdministration.entryRelationship.act"/>
            <comment value="SHALL contain exactly one [1..1] Instruction (identifier: urn:hl7ii:2.16.840.1.113883.10.20.22.4.20:2014-06-09) (CONF:1198-31392)."/>
