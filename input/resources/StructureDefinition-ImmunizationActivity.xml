            <type>
                <code value="http://hl7.org/cda/stds/core/StructureDefinition/ParticipantRole"/>
                <profile value="http://hl7.org/cda/us/ccda/StructureDefinition/DrugVehicle"/>
            </type>
        </element>
        <element id="SubstanceAdministration.entryRelationship">
