<?xml version="1.0" encoding="UTF-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
    <id value="SensoryStatus"/>
    <extension url="http://hl7.org/fhir/tools/StructureDefinition/logical-target">
        <valueBoolean>
            <extension url="http://hl7.org/fhir/StructureDefinition/data-absent-reason">
                <valueCode value="not-applicable"/>
            </extension>
        </valueBoolean>
    </extension>
    <extension url="http://hl7.org/fhir/tools/StructureDefinition/xml-namespace">
        <valueUri value="urn:hl7-org:v3"/>
    </extension>
    <extension url="http://hl7.org/fhir/tools/StructureDefinition/xml-name">
        <valueString value="observation"/>
    </extension>
    <extension url="http://hl7.org/fhir/tools/StructureDefinition/logical-container">
        <valueUri value="http://hl7.org/cda/stds/core/StructureDefinition/ClinicalDocument"/>
    </extension>
    <extension url="http://hl7.org/fhir/tools/StructureDefinition/type-profile-style">
        <valueCode value="cda"/>
    </extension>
    <url value="http://hl7.org/cda/us/ccda/StructureDefinition/SensoryStatus"/>
    <identifier>
        <system value="urn:ietf:rfc:3986"/>
        <value value="urn:oid:2.16.840.1.113883.10.20.22.4.127"/>
    </identifier>
    <name value="SensoryStatus"/>
    <title value="Sensory Status"/>
    <status value="draft"/>
    <description value="This template represents a patient's sensory or speech ability. It may contain an assessment scale observations related to the sensory or speech ability."/>
    <fhirVersion value="5.0.0"/>
    <kind value="logical"/>
    <abstract value="false"/>
    <type value="http://hl7.org/cda/stds/core/StructureDefinition/Observation"/>
    <baseDefinition value="http://hl7.org/cda/stds/core/StructureDefinition/Observation"/>
    <derivation value="constraint"/>
    <differential>
        <element id="Observation">
            <path value="Observation"/>
            <constraint>
                <key value="should-text-ref-value"/>
                <severity value="warning"/>
                <human value="SHOULD contain text/reference/@value"/>
                <expression value="text.reference.value.exists()"/>
                <source value="http://hl7.org/cda/us/ccda/StructureDefinition/SensoryStatus"/>
            </constraint>
            <constraint>
                <key value="should-author"/>
                <severity value="warning"/>
                <human value="SHOULD contain author"/>
                <expression value="author.exists()"/>
                <source value="http://hl7.org/cda/us/ccda/StructureDefinition/SensoryStatus"/>
            </constraint>
        </element>
        <element id="Observation.templateId">
            <path value="Observation.templateId"/>
            <slicing>
                <discriminator>
                    <type value="value"/>
                    <path value="root"/>
                </discriminator>
                <rules value="open"/>
            </slicing>
            <min value="1"/>
        </element>
        <element id="Observation.templateId:sensory-status">
            <path value="Observation.templateId"/>
            <sliceName value="sensory-status"/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Observation.templateId:sensory-status.root">
            <path value="Observation.templateId.root"/>
            <min value="1"/>
            <patternString value="2.16.840.1.113883.10.20.22.4.127"/>
        </element>
        <element id="Observation.templateId:sensory-status.extension">
            <path value="Observation.templateId.extension"/>
            <max value="0"/>
        </element>
        <element id="Observation.classCode">
            <path value="Observation.classCode"/>
            <comment value="SHALL contain exactly one [1..1] @classCode=&quot;OBS&quot; (CodeSystem: HL7ActClass urn:oid:2.16.840.1.113883.5.6) (CONF:1098-31017)."/>
            <fixedCode value="OBS"/>
        </element>
        <element id="Observation.moodCode">
            <path value="Observation.moodCode"/>
            <comment value="SHALL contain exactly one [1..1] @moodCode=&quot;EVN&quot; (CodeSystem: HL7ActMood urn:oid:2.16.840.1.113883.5.1001) (CONF:1098-31018)."/>
            <fixedCode value="EVN"/>
        </element>
        <element id="Observation.code">
            <path value="Observation.code"/>
            <comment value="SHALL contain exactly one [1..1] code, which SHOULD be selected from ValueSet Sensory Status Problem Type urn:oid:2.16.840.1.113883.11.20.9.50 DYNAMIC (CONF:1098-27962)."/>
            <binding>
                <strength value="preferred"/>
                <valueSet value="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.11.20.9.50"/>
            </binding>
        </element>
        <element id="Observation.text">
            <path value="Observation.text"/>
            <short value="SHOULD reference the portion of section narrative text corresponding to this entry"/>
        </element>
        <element id="Observation.text.reference">
            <path value="Observation.text.reference"/>
            <constraint>
                <key value="value-starts-octothorpe"/>
                <severity value="error"/>
                <human value="If reference/@value is present, it SHALL begin with a '#' and SHALL point to its corresponding narrative"/>
                <expression value="value.exists() implies value.startsWith('#')"/>
                <source value="http://hl7.org/cda/us/ccda/StructureDefinition/SensoryStatus"/>
            </constraint>
        </element>
        <element id="Observation.statusCode">
            <path value="Observation.statusCode"/>
            <comment value="SHALL contain exactly one [1..1] statusCode (CONF:1098-31437)."/>
            <min value="1"/>
        </element>
        <element id="Observation.statusCode.code">
            <path value="Observation.statusCode.code"/>
            <comment value="This statusCode SHALL contain exactly one [1..1] @code=&quot;completed&quot; (CodeSystem: HL7ActStatus urn:oid:2.16.840.1.113883.5.14) (CONF:1098-31438)."/>
            <min value="1"/>
            <fixedCode value="completed"/>
        </element>
        <element id="Observation.effectiveTime">
            <path value="Observation.effectiveTime"/>
            <comment value="SHALL contain exactly one [1..1] effectiveTime (CONF:1098-31441)."/>
            <min value="1"/>
        </element>
        <element id="Observation.effectiveTime.low">
            <path value="Observation.effectiveTime.low"/>
            <short value="The effectiveTime/low (a.k.a. &quot;onset date&quot;) asserts when the condition became biologically active."/>
            <comment value="This effectiveTime SHALL contain exactly one [1..1] low (CONF:1098-32630)."/>
            <min value="1"/>
        </element>
        <element id="Observation.effectiveTime.high">
            <path value="Observation.effectiveTime.high"/>
            <short value="The effectiveTime/high (a.k.a. &quot;resolution date&quot;) asserts when the condition became biologically resolved."/>
            <comment value="This effectiveTime MAY contain zero or one [0..1] high (CONF:1098-32631)."/>
        </element>
        <element id="Observation.value">
            <path value="Observation.value"/>
            <comment value="SHALL contain exactly one [1..1] value with @xsi:type=&quot;CD&quot;, where the code SHOULD be selected from ValueSet Mental and Functional Status Response urn:oid:2.16.840.1.113883.11.20.9.44 DYNAMIC (CONF:1098-27974)."/>
            <min value="1"/>
            <max value="1"/>
            <type>
                <code value="http://hl7.org/cda/stds/core/StructureDefinition/CD"/>
            </type>
            <binding>
                <strength value="preferred"/>
                <valueSet value="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.11.20.9.44"/>
            </binding>
        </element>
        <element id="Observation.author">
            <path value="Observation.author"/>
            <comment value="SHOULD contain zero or more [0..*] Author Participation (identifier: urn:oid:2.16.840.1.113883.10.20.22.4.119) (CONF:1098-31439)."/>
            <type>
                <code value="http://hl7.org/cda/stds/core/StructureDefinition/Author"/>
                <profile value="http://hl7.org/cda/us/ccda/StructureDefinition/AuthorParticipation"/>
            </type>
        </element>
        <element id="Observation.entryRelationship">
            <path value="Observation.entryRelationship"/>
            <slicing>
                <discriminator>
                    <type value="profile"/>
                    <path value="observation"/>
                </discriminator>
                <rules value="open"/>
            </slicing>
        </element>
        <element id="Observation.entryRelationship:assessmentScaleObservation">
            <path value="Observation.entryRelationship"/>
            <sliceName value="assessmentScaleObservation"/>
            <comment value="MAY contain zero or more [0..*] entryRelationship (CONF:1098-27984) such that it"/>
            <min value="0"/>
            <max value="*"/>
        </element>
        <element id="Observation.entryRelationship:assessmentScaleObservation.typeCode">
            <path value="Observation.entryRelationship.typeCode"/>
            <comment value="SHALL contain exactly one [1..1] @typeCode=&quot;SPRT&quot; has support (CodeSystem: HL7ActRelationshipType urn:oid:2.16.840.1.113883.5.1002 STATIC) (CONF:1098-27985)."/>
            <fixedCode value="SPRT"/>
        </element>
        <element id="Observation.entryRelationship:assessmentScaleObservation.observation">
            <path value="Observation.entryRelationship.observation"/>
            <comment value="SHALL contain exactly one [1..1] Assessment Scale Observation (identifier: urn:oid:2.16.840.1.113883.10.20.22.4.69) (CONF:1098-27986)."/>
            <min value="1"/>
            <type>
                <code value="http://hl7.org/cda/stds/core/StructureDefinition/Observation"/>
                <profile value="http://hl7.org/cda/us/ccda/StructureDefinition/AssessmentScaleObservation"/>
            </type>
        </element>
    </differential>
</StructureDefinition>