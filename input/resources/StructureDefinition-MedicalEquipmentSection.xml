            <min value="1"/>
            <patternString value="2.16.840.1.113883.6.1"/>
        </element>
        <element id="Section.title">
            <path value="Section.title"/>
            <comment value="SHALL contain exactly one [1..1] title (CONF:1098-7946)."/>
            <min value="1"/>
        </element>
        <element id="Section.text">
            <path value="Section.text"/>
            <comment value="SHALL contain exactly one [1..1] text (CONF:1098-7947)."/>
            <min value="1"/>
        </element>
        <element id="Section.entry">
