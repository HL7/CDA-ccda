                </discriminator>
                <discriminator>
                    <type value="value"/>
                    <path value="extension"/>
                </discriminator>
                <rules value="open"/>
            </slicing>
            <min value="1"/>
        </element>
        <element id="Section.templateId:primary">
            <path value="Section.templateId"/>
            <sliceName value="primary"/>
            <comment value="SHALL contain exactly one [1..1] templateId (CONF:1098-7944) such that it"/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Section.templateId:primary.root">
            <path value="Section.templateId.root"/>
            <comment value="SHALL contain exactly one [1..1] @root=&quot;2.16.840.1.113883.10.20.22.2.23&quot; (CONF:1098-10404)."/>
            <min value="1"/>
            <patternString value="2.16.840.1.113883.10.20.22.2.23"/>
        </element>
        <element id="Section.templateId:primary.extension">
            <path value="Section.templateId.extension"/>
            <comment value="SHALL contain exactly one [1..1] @extension=&quot;2014-06-09&quot; (CONF:1098-32523)."/>
            <min value="1"/>
            <patternString value="2014-06-09"/>
        </element>
        <element id="Section.code">
