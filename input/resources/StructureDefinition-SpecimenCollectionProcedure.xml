<?xml version="1.0" encoding="UTF-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
    <id value="SpecimenCollectionProcedure"/>
    <extension url="http://hl7.org/fhir/tools/StructureDefinition/logical-target">
        <valueBoolean>
            <extension url="http://hl7.org/fhir/StructureDefinition/data-absent-reason">
                <valueCode value="not-applicable"/>
            </extension>
        </valueBoolean>
    </extension>
    <extension url="http://hl7.org/fhir/StructureDefinition/elementdefinition-namespace">
        <valueUri value="urn:hl7-org:v3"/>
    </extension>
    <extension url="http://hl7.org/fhir/StructureDefinition/elementdefinition-xml-name">
        <valueUri value="procedure"/>
    </extension>
    <extension url="http://hl7.org/fhir/tools/StructureDefinition/type-profile-style">
        <valueUri value="cda"/>
    </extension>
    <url value="http://hl7.org/cda/us/ccda/StructureDefinition/SpecimenCollectionProcedure"/>
    <identifier>
        <value value="urn:hl7ii:2.16.840.1.113883.10.20.22.4.415:2018-09-01"/>
    </identifier>
    <version value="2018-09-01"/>
    <name value="SpecimenCollectionProcedure"/>
    <title value="Specimen Collection Procedure"/>
    <status value="draft"/>
    <description value="This template represents clinical information about the specimen including when it was collected, the type and source of specimen, as well as related observations regarding the specimen."/>
    <fhirVersion value="5.0.0"/>
    <mapping>
        <identity value="rim"/>
        <uri value="http://hl7.org/v3"/>
        <name value="RIM Mapping"/>
    </mapping>
    <kind value="logical"/>
    <abstract value="false"/>
    <type value="http://hl7.org/cda/stds/core/StructureDefinition/Procedure"/>
    <baseDefinition value="http://hl7.org/cda/stds/core/StructureDefinition/Procedure"/>
    <derivation value="constraint"/>
    <differential>
        <element id="Procedure">
            <path value="Procedure"/>
            <constraint>
                <key value="should-effectiveTime"/>
                <severity value="warning"/>
                <human value="SHOULD contain effectiveTime"/>
                <expression value="effectiveTime.exists()"/>
                <source value="http://hl7.org/cda/us/ccda/StructureDefinition/SpecimenCollectionProcedure"/>
            </constraint>
            <constraint>
                <key value="should-targetSiteCode"/>
                <severity value="warning"/>
                <human value="SHOULD contain targetSiteCode"/>
                <expression value="targetSiteCode.exists()"/>
                <source value="http://hl7.org/cda/us/ccda/StructureDefinition/SpecimenCollectionProcedure"/>
            </constraint>
        </element>
        <element id="Procedure.templateId">
            <path value="Procedure.templateId"/>
            <slicing>
                <discriminator>
                    <type value="value"/>
                    <path value="root"/>
                </discriminator>
                <discriminator>
                    <type value="value"/>
                    <path value="extension"/>
                </discriminator>
                <rules value="open"/>
            </slicing>
            <min value="1"/>
        </element>
        <element id="Procedure.templateId:specimen-collection-proc">
            <path value="Procedure.templateId"/>
            <sliceName value="specimen-collection-proc"/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Procedure.templateId:specimen-collection-proc.root">
            <path value="Procedure.templateId.root"/>
            <min value="1"/>
            <patternString value="2.16.840.1.113883.10.20.22.4.415"/>
        </element>
        <element id="Procedure.templateId:specimen-collection-proc.extension">
            <path value="Procedure.templateId.extension"/>
            <min value="1"/>
            <patternString value="2018-09-01"/>
        </element>
        <element id="Procedure.moodCode">
            <path value="Procedure.moodCode"/>
            <fixedCode value="EVN"/>
        </element>
        <element id="Procedure.code">
            <path value="Procedure.code"/>
            <short value="Specimen collection (procedure)"/>
            <min value="1"/>
        </element>
        <element id="Procedure.code.code">
            <path value="Procedure.code.code"/>
            <patternCode value="17636008"/>
        </element>
        <element id="Procedure.code.codeSystem">
            <path value="Procedure.code.codeSystem"/>
            <patternString value="2.16.840.1.113883.6.96"/>
        </element>
        <element id="Procedure.targetSiteCode">
            <extension url="http://hl7.org/fhir/us/core/StructureDefinition/uscdi-requirement">
                <valueBoolean value="true"/>
            </extension>
            <path value="Procedure.targetSiteCode"/>
            <short value="(USCDI) Specimen Source Site"/>
            <binding>
                <strength value="required"/>
                <valueSet value="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.3.88.12.3221.8.9"/>
            </binding>
        </element>
        <element id="Procedure.entryRelationship">
            <path value="Procedure.entryRelationship"/>
            <slicing>
                <discriminator>
                    <type value="profile"/>
                    <path value="act"/>
                </discriminator>
                <discriminator>
                    <type value="profile"/>
                    <path value="observation"/>
                </discriminator>
                <rules value="open"/>
            </slicing>
            <min value="1"/>
        </element>
        <element id="Procedure.entryRelationship:specimenRef">
            <path value="Procedure.entryRelationship"/>
            <sliceName value="specimenRef"/>
            <short value="References the specimen that is the subject of this procedure"/>
            <comment value="SHALL contain exactly one [1..1] entryRelationship such that it"/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Procedure.entryRelationship:specimenRef.typeCode">
            <path value="Procedure.entryRelationship.typeCode"/>
            <comment value="SHALL contain exactly one [1..1] @typeCode=&quot;SUBJ&quot; Has Subject (CodeSystem: HL7ActRelationshipType urn:oid:2.16.840.1.113883.5.1002 STATIC)."/>
            <fixedCode value="SUBJ"/>
        </element>
        <element id="Procedure.entryRelationship:specimenRef.act">
            <path value="Procedure.entryRelationship.act"/>
            <comment value="SHALL contain exactly one [1..1] Entry Reference (identifier: urn:hl7ii:2.16.840.1.113883.10.20.22.4.122)."/>
            <min value="1"/>
            <type>
                <code value="http://hl7.org/cda/stds/core/StructureDefinition/Act"/>
                <profile value="http://hl7.org/cda/us/ccda/StructureDefinition/EntryReference"/>
            </type>
        </element>
        <element id="Procedure.entryRelationship:rejectReason">
            <path value="Procedure.entryRelationship"/>
            <sliceName value="rejectReason"/>
            <comment value="SHOULD contain at least one [0..*] entryRelationship such that it"/>
            <min value="0"/>
            <max value="*"/>
        </element>
        <element id="Procedure.entryRelationship:rejectReason.typeCode">
            <path value="Procedure.entryRelationship.typeCode"/>
            <comment value="SHALL contain exactly one [1..1] @typeCode=&quot;COMP&quot; Component (CodeSystem: HL7ActRelationshipType urn:oid:2.16.840.1.113883.5.1002 STATIC)."/>
            <fixedCode value="COMP"/>
        </element>
        <element id="Procedure.entryRelationship:rejectReason.observation">
            <extension url="http://hl7.org/fhir/us/core/StructureDefinition/uscdi-requirement">
                <valueBoolean value="true"/>
            </extension>
            <path value="Procedure.entryRelationship.observation"/>
            <short value="(USCDI) Specimen Condition Acceptability"/>
            <comment value="SHALL contain exactly one [1..1] Specimen Reject Reason Observation (identifier: urn:hl7ii:2.16.840.1.113883.10.20.22.4.420:2018-09-01)."/>
            <min value="1"/>
            <type>
                <code value="http://hl7.org/cda/stds/core/StructureDefinition/Observation"/>
                <profile value="http://hl7.org/cda/us/ccda/StructureDefinition/SpecimenRejectReasonObservation"/>
            </type>
        </element>
    </differential>
</StructureDefinition>