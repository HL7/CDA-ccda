<?xml version="1.0" encoding="UTF-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
    <id value="PlannedImmunizationActivity"/>
    <extension url="http://hl7.org/fhir/tools/StructureDefinition/logical-target">
        <valueBoolean>
            <extension url="http://hl7.org/fhir/StructureDefinition/data-absent-reason">
                <valueCode value="not-applicable"/>
            </extension>
        </valueBoolean>
    </extension>
    <extension url="http://hl7.org/fhir/tools/StructureDefinition/xml-namespace">
        <valueUri value="urn:hl7-org:v3"/>
    </extension>
    <extension url="http://hl7.org/fhir/tools/StructureDefinition/xml-name">
        <valueString value="substanceAdministration"/>
    </extension>
    <extension url="http://hl7.org/fhir/tools/StructureDefinition/logical-container">
        <valueUri value="http://hl7.org/cda/stds/core/StructureDefinition/ClinicalDocument"/>
    </extension>
    <extension url="http://hl7.org/fhir/tools/StructureDefinition/type-profile-style">
        <valueUri value="cda"/>
    </extension>
    <url value="http://hl7.org/cda/us/ccda/StructureDefinition/PlannedImmunizationActivity"/>
    <identifier>
        <system value="urn:ietf:rfc:3986"/>
        <value value="urn:oid:2.16.840.1.113883.10.20.22.4.120"/>
    </identifier>
    <name value="PlannedImmunizationActivity"/>
    <title value="Planned Immunization Activity"/>
    <status value="draft"/>
    <description value="This template represents planned immunizations. Planned Immunization Activity is very similar to Planned Medication Activity with some key differences, for example, the drug code system is constrained to CVX codes. &#xA;The priority of the immunization activity to the patient and provider is communicated through Priority Preference. The effectiveTime indicates the time when the immunization activity is intended to take place and authorTime indicates when the documentation of the plan occurred."/>
    <fhirVersion value="5.0.0"/>
    <kind value="logical"/>
    <abstract value="false"/>
    <type value="http://hl7.org/cda/stds/core/StructureDefinition/SubstanceAdministration"/>
    <baseDefinition value="http://hl7.org/cda/stds/core/StructureDefinition/SubstanceAdministration"/>
    <derivation value="constraint"/>
    <differential>
        <element id="SubstanceAdministration">
            <path value="SubstanceAdministration"/>
            <constraint>
                <key value="should-text-ref-value"/>
                <severity value="warning"/>
                <human value="SHOULD contain text/reference/@value"/>
                <expression value="text.reference.value.exists()"/>
                <source value="http://hl7.org/cda/us/ccda/StructureDefinition/PlannedImmunizationActivity"/>
            </constraint>
        </element>
        <element id="SubstanceAdministration.templateId">
            <path value="SubstanceAdministration.templateId"/>
            <slicing>
                <discriminator>
                    <type value="value"/>
                    <path value="root"/>
                </discriminator>
                <rules value="open"/>
            </slicing>
            <min value="1"/>
        </element>
        <element id="SubstanceAdministration.templateId:planned-immunization-activity">
            <path value="SubstanceAdministration.templateId"/>
            <sliceName value="planned-immunization-activity"/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="SubstanceAdministration.templateId:planned-immunization-activity.root">
            <path value="SubstanceAdministration.templateId.root"/>
            <min value="1"/>
            <patternString value="2.16.840.1.113883.10.20.22.4.120"/>
        </element>
        <element id="SubstanceAdministration.templateId:planned-immunization-activity.extension">
            <path value="SubstanceAdministration.templateId.extension"/>
            <max value="0"/>
        </element>
        <element id="SubstanceAdministration.classCode">
            <path value="SubstanceAdministration.classCode"/>
            <comment value="SHALL contain exactly one [1..1] @classCode=&quot;SBADM&quot; (CONF:1098-32091)."/>
        </element>
        <element id="SubstanceAdministration.moodCode">
            <path value="SubstanceAdministration.moodCode"/>
            <comment value="SHALL contain exactly one [1..1] @moodCode, which SHALL be selected from ValueSet Planned moodCode (SubstanceAdministration/Supply) urn:oid:2.16.840.1.113883.11.20.9.24 STATIC 2014-09-01 (CONF:1098-32097)."/>
            <binding>
                <strength value="required"/>
                <valueSet value="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.11.20.9.24"/>
            </binding>
        </element>
        <element id="SubstanceAdministration.id">
            <path value="SubstanceAdministration.id"/>
            <comment value="SHALL contain at least one [1..*] id (CONF:1098-32100)."/>
            <min value="1"/>
        </element>
        <element id="SubstanceAdministration.text">
            <path value="SubstanceAdministration.text"/>
            <short value="SHOULD reference the portion of section narrative text corresponding to this entry"/>
        </element>
        <element id="SubstanceAdministration.text.reference">
            <path value="SubstanceAdministration.text.reference"/>
            <constraint>
                <key value="value-starts-octothorpe"/>
                <severity value="error"/>
                <human value="If reference/@value is present, it SHALL begin with a '#' and SHALL point to its corresponding narrative"/>
                <expression value="value.exists() implies value.startsWith('#')"/>
                <source value="http://hl7.org/cda/us/ccda/StructureDefinition/PlannedImmunizationActivity"/>
            </constraint>
        </element>
        <element id="SubstanceAdministration.statusCode">
            <path value="SubstanceAdministration.statusCode"/>
            <comment value="SHALL contain exactly one [1..1] statusCode (CONF:1098-32101)."/>
            <min value="1"/>
        </element>
        <element id="SubstanceAdministration.statusCode.code">
            <path value="SubstanceAdministration.statusCode.code"/>
            <comment value="This statusCode SHALL contain exactly one [1..1] @code=&quot;active&quot; Active (CodeSystem: HL7ActStatus urn:oid:2.16.840.1.113883.5.14) (CONF:1098-32102)."/>
            <min value="1"/>
            <fixedCode value="active"/>
        </element>
        <element id="SubstanceAdministration.effectiveTime">
            <path value="SubstanceAdministration.effectiveTime"/>
            <short value="The effectiveTime in a planned immunization activity represents the time that the immunization activity should occur."/>
            <comment value="SHALL contain exactly one [1..1] effectiveTime (CONF:1098-32103)."/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="SubstanceAdministration.repeatNumber">
            <path value="SubstanceAdministration.repeatNumber"/>
            <short value="In a Planned Immunization Activity, repeatNumber defines the number of allowed administrations. For example, a repeatNumber of &quot;3&quot; means that the substance can be administered up to 3 times."/>
            <comment value="MAY contain zero or one [0..1] repeatNumber (CONF:1098-32126)."/>
        </element>
        <element id="SubstanceAdministration.routeCode">
            <path value="SubstanceAdministration.routeCode"/>
            <comment value="MAY contain zero or one [0..1] routeCode, which SHALL be selected from ValueSet SPL Drug Route of Administration Terminology urn:oid:2.16.840.1.113883.3.88.12.3221.8.7 DYNAMIC (CONF:1098-32127)."/>
            <constraint>
                <key value="should-translation"/>
                <severity value="warning"/>
                <human value="SHOULD contain translation"/>
                <expression value="translation.exists()"/>
                <source value="http://hl7.org/cda/us/ccda/StructureDefinition/PlannedImmunizationActivity"/>
            </constraint>
            <binding>
                <strength value="required"/>
                <valueSet value="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.3.88.12.3221.8.7"/>
            </binding>
        </element>
        <element id="SubstanceAdministration.routeCode.translation">
            <path value="SubstanceAdministration.routeCode.translation"/>
            <comment value="The routeCode, if present, SHOULD contain zero or more [0..*] translation, which SHOULD be selected from ValueSet Medication Route urn:oid:2.16.840.1.113762.1.4.1099.12 DYNAMIC (CONF:1098-32951)."/>
            <binding>
                <strength value="preferred"/>
                <valueSet value="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113762.1.4.1099.12"/>
            </binding>
        </element>
        <element id="SubstanceAdministration.approachSiteCode">
            <path value="SubstanceAdministration.approachSiteCode"/>
            <comment value="MAY contain zero or more [0..*] approachSiteCode, which SHALL be selected from ValueSet Body Site Value Set urn:oid:2.16.840.1.113883.3.88.12.3221.8.9 DYNAMIC (CONF:1098-32128)."/>
            <binding>
                <strength value="required"/>
                <valueSet value="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.3.88.12.3221.8.9"/>
            </binding>
        </element>
        <element id="SubstanceAdministration.doseQuantity">
            <path value="SubstanceAdministration.doseQuantity"/>
            <comment value="MAY contain zero or one [0..1] doseQuantity (CONF:1098-32129)."/>
            <constraint>
                <key value="should-unit"/>
                <severity value="warning"/>
                <human value="SHOULD contain @unit"/>
                <expression value="nullFlavor.exists() or unit.exists()"/>
                <source value="http://hl7.org/cda/us/ccda/StructureDefinition/PlannedImmunizationActivity"/>
            </constraint>
        </element>
        <element id="SubstanceAdministration.doseQuantity.unit">
            <path value="SubstanceAdministration.doseQuantity.unit"/>
            <short value="NOTE: The base CDA R2.0 standard requires @unit to be drawn from UCUM, and best practice is to use case sensitive UCUM units"/>
            <comment value="The doseQuantity, if present, SHOULD contain zero or one [0..1] @unit, which SHOULD be selected from ValueSet UnitsOfMeasureCaseSensitive urn:oid:2.16.840.1.113883.1.11.12839 DYNAMIC (CONF:1098-32130)."/>
            <binding>
                <strength value="preferred"/>
                <valueSet value="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.1.11.12839"/>
            </binding>
        </element>
        <element id="SubstanceAdministration.consumable">
            <path value="SubstanceAdministration.consumable"/>
            <comment value="SHALL contain exactly one [1..1] consumable (CONF:1098-32131)."/>
        </element>
        <element id="SubstanceAdministration.consumable.manufacturedProduct">
            <path value="SubstanceAdministration.consumable.manufacturedProduct"/>
            <comment value="This consumable SHALL contain exactly one [1..1] Immunization Medication Information (identifier: urn:hl7ii:2.16.840.1.113883.10.20.22.4.54:2014-06-09) (CONF:1098-32132)."/>
            <type>
                <code value="http://hl7.org/cda/stds/core/StructureDefinition/ManufacturedProduct"/>
                <profile value="http://hl7.org/cda/us/ccda/StructureDefinition/ImmunizationMedicationInformation"/>
            </type>
        </element>
        <element id="SubstanceAdministration.performer">
            <path value="SubstanceAdministration.performer"/>
            <short value="The clinician who is expected to perform the planned immunization activity could be identified using substanceAdministration/performer."/>
            <comment value="MAY contain zero or more [0..*] performer (CONF:1098-32104)."/>
        </element>
        <element id="SubstanceAdministration.author">
            <path value="SubstanceAdministration.author"/>
            <short value="The author in a planned immunization activity represents the clinician who is requesting or planning the immunization activity."/>
            <comment value="MAY contain zero or more [0..*] Author Participation (identifier: urn:oid:2.16.840.1.113883.10.20.22.4.119) (CONF:1098-32105)."/>
            <type>
                <code value="http://hl7.org/cda/stds/core/StructureDefinition/Author"/>
                <profile value="http://hl7.org/cda/us/ccda/StructureDefinition/AuthorParticipation"/>
            </type>
        </element>
        <element id="SubstanceAdministration.entryRelationship">
            <path value="SubstanceAdministration.entryRelationship"/>
            <slicing>
                <discriminator>
                    <type value="profile"/>
                    <path value="observation"/>
                </discriminator>
                <discriminator>
                    <type value="profile"/>
                    <path value="act"/>
                </discriminator>
                <rules value="open"/>
            </slicing>
            <short value="The following entryRelationship captures any instructions associated with the planned immunization activity."/>
        </element>
        <element id="SubstanceAdministration.entryRelationship:priorityPreference">
            <path value="SubstanceAdministration.entryRelationship"/>
            <sliceName value="priorityPreference"/>
            <short value="The following entryRelationship represents the priority that a patient or a provider places on the immunization activity."/>
            <comment value="MAY contain zero or more [0..*] entryRelationship (CONF:1098-32108) such that it"/>
            <min value="0"/>
            <max value="*"/>
        </element>
        <element id="SubstanceAdministration.entryRelationship:priorityPreference.typeCode">
            <path value="SubstanceAdministration.entryRelationship.typeCode"/>
            <comment value="SHALL contain exactly one [1..1] @typeCode=&quot;REFR&quot; Refers to (CodeSystem: HL7ActRelationshipType urn:oid:2.16.840.1.113883.5.1002) (CONF:1098-32109)."/>
            <fixedCode value="REFR"/>
        </element>
        <element id="SubstanceAdministration.entryRelationship:priorityPreference.observation">
            <path value="SubstanceAdministration.entryRelationship.observation"/>
            <comment value="SHALL contain exactly one [1..1] Priority Preference (identifier: urn:oid:2.16.840.1.113883.10.20.22.4.143) (CONF:1098-32110)."/>
            <min value="1"/>
            <type>
                <code value="http://hl7.org/cda/stds/core/StructureDefinition/Observation"/>
                <profile value="http://hl7.org/cda/us/ccda/StructureDefinition/PriorityPreference"/>
            </type>
        </element>
        <element id="SubstanceAdministration.entryRelationship:indication">
            <path value="SubstanceAdministration.entryRelationship"/>
            <sliceName value="indication"/>
            <short value="The following entryRelationship represents the indication for the immunization activity."/>
            <comment value="MAY contain zero or more [0..*] entryRelationship (CONF:1098-32114) such that it"/>
            <min value="0"/>
            <max value="*"/>
        </element>
        <element id="SubstanceAdministration.entryRelationship:indication.typeCode">
            <path value="SubstanceAdministration.entryRelationship.typeCode"/>
            <comment value="SHALL contain exactly one [1..1] @typeCode=&quot;RSON&quot; Has Reason (CodeSystem: HL7ActRelationshipType urn:oid:2.16.840.1.113883.5.1002) (CONF:1098-32115)."/>
            <fixedCode value="RSON"/>
        </element>
        <element id="SubstanceAdministration.entryRelationship:indication.observation">
            <path value="SubstanceAdministration.entryRelationship.observation"/>
            <comment value="SHALL contain exactly one [1..1] Indication (identifier: urn:hl7ii:2.16.840.1.113883.10.20.22.4.19:2014-06-09) (CONF:1098-32116)."/>
            <min value="1"/>
            <type>
                <code value="http://hl7.org/cda/stds/core/StructureDefinition/Observation"/>
                <profile value="http://hl7.org/cda/us/ccda/StructureDefinition/Indication"/>
            </type>
        </element>
        <element id="SubstanceAdministration.entryRelationship:instruction">
            <path value="SubstanceAdministration.entryRelationship"/>
            <sliceName value="instruction"/>
            <comment value="MAY contain zero or more [0..*] entryRelationship (CONF:1098-32117) such that it"/>
            <min value="0"/>
            <max value="*"/>
        </element>
        <element id="SubstanceAdministration.entryRelationship:instruction.typeCode">
            <path value="SubstanceAdministration.entryRelationship.typeCode"/>
            <comment value="SHALL contain exactly one [1..1] @typeCode=&quot;SUBJ&quot; Has Subject (CodeSystem: HL7ActRelationshipType urn:oid:2.16.840.1.113883.5.1002) (CONF:1098-32118)."/>
            <fixedCode value="SUBJ"/>
        </element>
        <element id="SubstanceAdministration.entryRelationship:instruction.act">
            <path value="SubstanceAdministration.entryRelationship.act"/>
            <comment value="SHALL contain exactly one [1..1] Instruction (identifier: urn:hl7ii:2.16.840.1.113883.10.20.22.4.20:2014-06-09) (CONF:1098-32119)."/>
            <min value="1"/>
            <type>
                <code value="http://hl7.org/cda/stds/core/StructureDefinition/Act"/>
                <profile value="http://hl7.org/cda/us/ccda/StructureDefinition/Instruction"/>
            </type>
        </element>
        <element id="SubstanceAdministration.precondition">
            <path value="SubstanceAdministration.precondition"/>
            <slicing>
                <discriminator>
                    <type value="profile"/>
                    <path value="criterion"/>
                </discriminator>
                <rules value="open"/>
            </slicing>
        </element>
        <element id="SubstanceAdministration.precondition:pronditionForSubstanceAdmin">
            <path value="SubstanceAdministration.precondition"/>
            <sliceName value="pronditionForSubstanceAdmin"/>
            <comment value="MAY contain zero or more [0..*] precondition (CONF:1098-32123) such that it"/>
            <min value="0"/>
            <max value="*"/>
        </element>
        <element id="SubstanceAdministration.precondition:pronditionForSubstanceAdmin.typeCode">
            <path value="SubstanceAdministration.precondition.typeCode"/>
            <comment value="SHALL contain exactly one [1..1] @typeCode=&quot;PRCN&quot; Precondition (CodeSystem: HL7ActRelationshipType urn:oid:2.16.840.1.113883.5.1002) (CONF:1098-32124)."/>
            <min value="1"/>
        </element>
        <element id="SubstanceAdministration.precondition:pronditionForSubstanceAdmin.criterion">
            <path value="SubstanceAdministration.precondition.criterion"/>
            <comment value="SHALL contain exactly one [1..1] Precondition for Substance Administration (identifier: urn:hl7ii:2.16.840.1.113883.10.20.22.4.25:2014-06-09) (CONF:1098-32125)."/>
            <type>
                <code value="http://hl7.org/cda/stds/core/StructureDefinition/Criterion"/>
                <profile value="http://hl7.org/cda/us/ccda/StructureDefinition/PreconditionforSubstanceAdministration"/>
            </type>
        </element>
    </differential>
</StructureDefinition>