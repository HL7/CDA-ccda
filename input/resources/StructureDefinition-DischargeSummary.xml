                <expression value="assignedPerson.exists() or representedOrganization.exists()"/>
                <source value="http://hl7.org/cda/us/ccda/StructureDefinition/DischargeSummary"/>
            </constraint>
        </element>
        <element id="ClinicalDocument.component">
