    </identifier>
    <version value="2014-06-09"/>
    <name value="PlannedMedicationActivity"/>
    <title value="Planned Medication Activity"/>
    <status value="active"/>
    <contact>
        <name value="Gaye Dolin"/>
        <telecom>
