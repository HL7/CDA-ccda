    <description value="This template represents information about the overall health status of the patient. To represent the impact of a specific problem or concern related to the patient's expected health outcome use the Prognosis Observation template 2.16.840.1.113883.10.20.22.4.113."/>
    <fhirVersion value="4.0.1"/>
    <kind value="logical"/>
    <abstract value="false"/>
    <type value="http://hl7.org/cda/stds/core/StructureDefinition/Observation"/>
    <baseDefinition value="http://hl7.org/cda/stds/core/StructureDefinition/Observation"/>
    <derivation value="constraint"/>
    <differential>
        <element id="Observation">
            <path value="Observation"/>
        </element>
        <element id="Observation.classCode">
            <path value="Observation.classCode"/>
            <requirements value="SHALL contain exactly one [1..1] @classCode=&quot;OBS&quot; Observation (CodeSystem: HL7ActClass urn:oid:2.16.840.1.113883.5.6 STATIC) (CONF:1098-9057)."/>
            <min value="1"/>
            <max value="1"/>
            <patternCode value="OBS"/>
        </element>
        <element id="Observation.moodCode">
            <path value="Observation.moodCode"/>
            <requirements value="SHALL contain exactly one [1..1] @moodCode=&quot;EVN&quot; Event (CodeSystem: HL7ActMood urn:oid:2.16.840.1.113883.5.1001 STATIC) (CONF:1098-9072)."/>
            <min value="1"/>
            <max value="1"/>
            <patternCode value="EVN"/>
        </element>
        <element id="Observation.templateId">
            <path value="Observation.templateId"/>
            <slicing>
                <discriminator>
                    <type value="value"/>
                    <path value="root"/>
                </discriminator>
                <discriminator>
                    <type value="value"/>
                    <path value="extension"/>
                </discriminator>
                <rules value="open"/>
            </slicing>
            <min value="1"/>
        </element>
        <element id="Observation.templateId:primary">
            <path value="Observation.templateId"/>
            <sliceName value="primary"/>
            <requirements value="SHALL contain exactly one [1..1] templateId (CONF:1098-16756) such that it"/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Observation.templateId:primary.root">
            <path value="Observation.templateId.root"/>
            <requirements value="SHALL contain exactly one [1..1] @root=&quot;2.16.840.1.113883.10.20.22.4.5&quot; (CONF:1098-16757)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="2.16.840.1.113883.10.20.22.4.5"/>
        </element>
        <element id="Observation.templateId:primary.extension">
            <path value="Observation.templateId.extension"/>
            <requirements value="SHALL contain exactly one [1..1] @extension=&quot;2014-06-09&quot; (CONF:1098-32558)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="2014-06-09"/>
        </element>
        <element id="Observation.id">
            <path value="Observation.id"/>
            <requirements value="SHALL contain at least one [1..*] id (CONF:1098-32486)."/>
            <min value="1"/>
            <max value="*"/>
        </element>
        <element id="Observation.code">
            <path value="Observation.code"/>
            <requirements value="SHALL contain exactly one [1..1] code (CONF:1098-19143)."/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Observation.code.code">
            <path value="Observation.code.code"/>
            <requirements value="This code SHALL contain exactly one [1..1] @code=&quot;11323-3&quot; Health status (CONF:1098-19144)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="11323-3"/>
        </element>
        <element id="Observation.code.codeSystem">
            <path value="Observation.code.codeSystem"/>
            <requirements value="This code SHALL contain exactly one [1..1] @codeSystem=&quot;2.16.840.1.113883.6.1&quot; (CodeSystem: LOINC urn:oid:2.16.840.1.113883.6.1) (CONF:1098-32161)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="2.16.840.1.113883.6.1"/>
        </element>
        <element id="Observation.statusCode">
            <path value="Observation.statusCode"/>
            <requirements value="SHALL contain exactly one [1..1] statusCode (CONF:1098-9074)."/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Observation.statusCode.code">
            <path value="Observation.statusCode.code"/>
            <requirements value="This statusCode SHALL contain exactly one [1..1] @code=&quot;completed&quot; Completed (CodeSystem: HL7ActStatus urn:oid:2.16.840.1.113883.5.14 STATIC) (CONF:1098-19103)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="completed"/>
        </element>
        <element id="Observation.value">
            <path value="Observation.value"/>
            <requirements value="SHALL contain exactly one [1..1] value with @xsi:type=&quot;CD&quot;, where the code SHALL be selected from ValueSet HealthStatus urn:oid:2.16.840.1.113883.1.11.20.12 DYNAMIC (CONF:1098-9075)."/>
            <min value="1"/>
            <max value="1"/>
            <type>
                <code value="http://hl7.org/cda/stds/core/StructureDefinition/CD"/>
            </type>
            <binding>
                <strength value="required"/>
                <valueSet value="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.1.11.20.12"/>
            </binding>
        </element>
    </differential>
</StructureDefinition>