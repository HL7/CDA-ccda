<StructureDefinition
    xmlns="http://hl7.org/fhir">
    <id value="2.16.840.1.113883.10.20.22.4.5"/>
    <url value="http://hl7.org/cda/us/ccda/StructureDefinition/2.16.840.1.113883.10.20.22.4.5"/>
    <identifier>
        <value value="urn:hl7ii:2.16.840.1.113883.10.20.22.4.5:2014-06-09"/>
    </identifier>
    <version value="2014-06-09"/>
    <name value="HealthStatusObservation"/>
    <title value="Health Status Observation"/>
    <status value="active"/>
    <contact>
        <name value="Gaye Dolin"/>
        <telecom>
            <system value="phone"/>
            <value value="714-744-4152"/>
            <use value="work"/>
        </telecom>
        <telecom>
            <system value="email"/>
            <value value="gaye.dolin@lantanagroup.com"/>
