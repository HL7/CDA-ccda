            <sliceName value="participant1"/>
            <short value="name"/>
            <definition value="The playingEntity, if present, SHALL contain at least one [1..*] name (CONF:1098-32394)."/>
            <min value="1"/>
            <max value="*"/>
        </element>
    </differential>
