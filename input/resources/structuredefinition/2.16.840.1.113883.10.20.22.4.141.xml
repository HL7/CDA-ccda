            <max value="1"/>
            <binding>
                <strength value="preferred"/>
                <valueSet value="https://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.114222.4.11.1066"/>
            </binding>
        </element>
        <element id="Act.participant:participant1.participantRole.addr">
