    <description value="This template represents priority preferences chosen by a patient or a care provider. Priority preferences are choices made by care providers or patients or both relative to options for care or treatment (including scheduling, care experience, and meeting of personal health goals), the sharing and disclosure of health information, and the prioritization of concerns and problems."/>
    <fhirVersion value="4.0.1"/>
    <kind value="logical"/>
    <abstract value="false"/>
    <type value="http://hl7.org/cda/stds/core/StructureDefinition/Observation"/>
    <baseDefinition value="http://hl7.org/cda/stds/core/StructureDefinition/Observation"/>
    <derivation value="constraint"/>
    <differential>
        <element id="Observation">
            <path value="Observation"/>
        </element>
        <element id="Observation.classCode">
            <path value="Observation.classCode"/>
            <requirements value="SHALL contain exactly one [1..1] @classCode=&quot;OBS&quot; (CodeSystem: HL7ActClass urn:oid:2.16.840.1.113883.5.6) (CONF:1098-30949)."/>
            <min value="1"/>
            <max value="1"/>
            <patternCode value="OBS"/>
        </element>
        <element id="Observation.moodCode">
            <path value="Observation.moodCode"/>
            <requirements value="SHALL contain exactly one [1..1] @moodCode=&quot;EVN&quot; (CodeSystem: HL7ActMood urn:oid:2.16.840.1.113883.5.1001) (CONF:1098-30950)."/>
            <min value="1"/>
            <max value="1"/>
            <patternCode value="EVN"/>
        </element>
        <element id="Observation.templateId">
            <path value="Observation.templateId"/>
            <slicing>
                <discriminator>
                    <type value="value"/>
                    <path value="root"/>
                </discriminator>
                <rules value="open"/>
            </slicing>
            <min value="1"/>
        </element>
        <element id="Observation.templateId:primary">
            <path value="Observation.templateId"/>
            <sliceName value="primary"/>
            <requirements value="SHALL contain exactly one [1..1] templateId (CONF:1098-30951) such that it"/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Observation.templateId:primary.root">
            <path value="Observation.templateId.root"/>
            <requirements value="SHALL contain exactly one [1..1] @root=&quot;2.16.840.1.113883.10.20.22.4.143&quot; (CONF:1098-30952)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="2.16.840.1.113883.10.20.22.4.143"/>
        </element>
        <element id="Observation.id">
            <path value="Observation.id"/>
            <requirements value="SHALL contain at least one [1..*] id (CONF:1098-30953)."/>
            <min value="1"/>
            <max value="*"/>
        </element>
        <element id="Observation.code">
            <path value="Observation.code"/>
            <requirements value="SHALL contain exactly one [1..1] code (CONF:1098-30954)."/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Observation.code.code">
            <path value="Observation.code.code"/>
            <requirements value="This code SHALL contain exactly one [1..1] @code=&quot;225773000&quot; Preference (CONF:1098-30955)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="225773000"/>
        </element>
        <element id="Observation.code.codeSystem">
            <path value="Observation.code.codeSystem"/>
            <requirements value="This code SHALL contain exactly one [1..1] @codeSystem=&quot;2.16.840.1.113883.6.96&quot; (CodeSystem: SNOMED CT urn:oid:2.16.840.1.113883.6.96) (CONF:1098-30956)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="2.16.840.1.113883.6.96"/>
        </element>
        <element id="Observation.effectiveTime">
            <path value="Observation.effectiveTime"/>
            <requirements value="SHOULD contain zero or one [0..1] effectiveTime (CONF:1098-32327)."/>
            <min value="0"/>
            <max value="1"/>
        </element>
        <element id="Observation.value">
            <path value="Observation.value"/>
            <requirements value="SHALL contain exactly one [1..1] value with @xsi:type=&quot;CD&quot;, where the code SHALL be selected from ValueSet Priority Level urn:oid:2.16.840.1.113883.11.20.9.60 DYNAMIC (CONF:1098-30957)."/>
            <min value="1"/>
            <max value="1"/>
            <type>
                <code value="http://hl7.org/cda/stds/core/StructureDefinition/CD"/>
            </type>
            <binding>
                <strength value="required"/>
                <valueSet value="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.11.20.9.60"/>
            </binding>
        </element>
        <element id="Observation.author">
            <path value="Observation.author"/>
            <requirements value="SHOULD contain zero or more [0..*] Author Participation (identifier: urn:oid:2.16.840.1.113883.10.20.22.4.119) (CONF:1098-30958)."/>
            <min value="0"/>
            <max value="*"/>
            <type>
                <code value="http://hl7.org/cda/stds/core/StructureDefinition/Author"/>
                <profile value="http://hl7.org/cda/us/ccda/StructureDefinition/2.16.840.1.113883.10.20.22.4.119"/>
            </type>
        </element>
    </differential>
</StructureDefinition>