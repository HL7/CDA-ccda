    <identifier>
        <value value="urn:oid:2.16.840.1.113883.10.20.22.4.72"/>
    </identifier>
    <name value="CaregiverCharacteristics"/>
    <title value="Caregiver Characteristics"/>
    <status value="active"/>
    <contact>
        <name value="Gaye Dolin"/>
        <telecom>
            <system value="phone"/>
