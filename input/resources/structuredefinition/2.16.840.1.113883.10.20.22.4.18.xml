<?xml version="1.0" encoding="UTF-8"?>
<StructureDefinition
    xmlns="http://hl7.org/fhir">
    <id value="2.16.840.1.113883.10.20.22.4.18"/>
    <url value="http://hl7.org/fhir/cda/ccda/StructureDefinition/2.16.840.1.113883.10.20.22.4.18"/>
    <identifier>
        <value value="urn:hl7ii:2.16.840.1.113883.10.20.22.4.18:2014-06-09"/>
    </identifier>
    <version value="2014-06-09"/>
    <name value="MedicationDispense"/>
    <title value="Medication Dispense (V2)"/>
    <status value="active"/>
    <contact>
        <name value="Gaye Dolin"/>
        <telecom>
            <system value="phone"/>
            <value value="714-744-4152"/>
            <use value="work"/>
        </telecom>
        <telecom>
            <system value="email"/>
            <value value="gaye.dolin@lantanagroup.com"/>
            <use value="work"/>
        </telecom>
    </contact>
    <description value="This template records the act of supplying medications (i.e., dispensing)."/>
    <kind value="resource"/>
    <abstract value="false"/>
    <type value="Supply"/>
    <baseDefinition value="http://hl7.org/fhir/cda/StructureDefinition/Supply"/>
    <derivation value="constraint"/>
    <differential>
        <element id="Supply">
            <path value="Supply"/>
            <constraint>
                <key value="1098-9333"/>
                <severity value="error"/>
                <human value="A supply act  **SHALL** contain one product/Medication Information *OR* one product/Immunization Medication Information template (CONF:1098-9333)."/>
            </constraint>
        </element>
        <element id="Supply.classCode">
            <path value="Supply.classCode"/>
            <short value="classCode"/>
            <definition value="SHALL contain exactly one [1..1] @classCode=&quot;SPLY&quot; (CodeSystem: HL7ActClass urn:oid:2.16.840.1.113883.5.6 STATIC) (CONF:1098-7451)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="SPLY"/>
        </element>
        <element id="Supply.moodCode">
            <path value="Supply.moodCode"/>
            <short value="moodCode"/>
            <definition value="SHALL contain exactly one [1..1] @moodCode=&quot;EVN&quot; (CodeSystem: HL7ActMood urn:oid:2.16.840.1.113883.5.1001 STATIC) (CONF:1098-7452)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="EVN"/>
        </element>
        <element id="Supply.templateId">
            <path value="Supply.templateId"/>
            <slicing>
                <discriminator>
                    <type value="value"/>
                    <path value="root"/>
                </discriminator>
                <discriminator>
                    <type value="value"/>
                    <path value="extension"/>
                </discriminator>
                <rules value="open"/>
            </slicing>
            <definition value="SHALL contain exactly one [1..1] templateId (CONF:1098-7453) such that it"/>
        </element>
        <element id="Supply.templateId:templateId1">
            <path value="Supply.templateId"/>
            <sliceName value="templateId1"/>
            <short value="templateId"/>
            <definition value="SHALL contain exactly one [1..1] templateId (CONF:1098-7453) such that it"/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Supply.templateId:templateId1.root">
            <path value="Supply.templateId.root"/>
            <short value="root"/>
            <definition value="SHALL contain exactly one [1..1] @root=&quot;2.16.840.1.113883.10.20.22.4.18&quot; (CONF:1098-10505)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="2.16.840.1.113883.10.20.22.4.18"/>
        </element>
        <element id="Supply.templateId:templateId1.extension">
            <path value="Supply.templateId.extension"/>
            <short value="extension"/>
            <definition value="SHALL contain exactly one [1..1] @extension=&quot;2014-06-09&quot; (CONF:1098-32580)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="2014-06-09"/>
        </element>
        <element id="Supply.id">
            <path value="Supply.id"/>
            <short value="id"/>
            <definition value="SHALL contain at least one [1..*] id (CONF:1098-7454)."/>
            <min value="1"/>
            <max value="*"/>
        </element>
        <element id="Supply.statusCode">
            <path value="Supply.statusCode"/>
            <short value="statusCode"/>
            <definition value="SHALL contain exactly one [1..1] statusCode (CONF:1098-7455)."/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Supply.statusCode.code">
            <path value="Supply.statusCode.code"/>
            <short value="code"/>
            <definition value="This statusCode SHALL contain exactly one [1..1] @code, which SHALL be selected from ValueSet Medication Fill Status urn:oid:2.16.840.1.113883.3.88.12.80.64 STATIC 2014-04-23 (CONF:1098-32361)."/>
            <min value="1"/>
            <max value="1"/>
            <binding>
                <strength value="required"/>
                <valueSet value="https://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.3.88.12.80.64"/>
            </binding>
        </element>
        <element id="Supply.effectiveTime">
            <path value="Supply.effectiveTime"/>
            <short value="effectiveTime"/>
            <definition value="SHOULD contain zero or one [0..1] effectiveTime (CONF:1098-7456)."/>
            <min value="0"/>
            <max value="1"/>
        </element>
        <element id="Supply.repeatNumber">
            <path value="Supply.repeatNumber"/>
            <short value="repeatNumber"/>
            <definition value="SHOULD contain zero or one [0..1] repeatNumber (CONF:1098-7457)."/>
            <min value="0"/>
            <max value="1"/>
        </element>
        <element id="Supply.quantity">
            <path value="Supply.quantity"/>
            <short value="quantity"/>
            <definition value="SHOULD contain zero or one [0..1] quantity (CONF:1098-7458)."/>
            <min value="0"/>
            <max value="1"/>
        </element>
        <element id="Supply.product">
            <path value="Supply.product"/>
            <short value="product"/>
            <definition value="MAY contain zero or one [0..1] product (CONF:1098-7459)."/>
            <min value="0"/>
            <max value="1"/>
        </element>
        <element id="Supply.product.manufacturedProduct">
            <path value="Supply.product.manufacturedProduct"/>
            <short value="manufacturedProduct"/>
            <definition value="The product, if present, SHALL contain exactly one [1..1] Medication Information (V2) (identifier: urn:hl7ii:2.16.840.1.113883.10.20.22.4.23:2014-06-09) (CONF:1098-15607)."/>
            <min value="1"/>
            <max value="1"/>
            <type>
                <code value="http://hl7.org/fhir/cda/StructureDefinition/ManufacturedProduct"/>
                <profile value="http://hl7.org/fhir/cda/ccda/StructureDefinition/2.16.840.1.113883.10.20.22.4.23"/>
            </type>
            <type>
                <code value="http://hl7.org/fhir/cda/StructureDefinition/ManufacturedProduct"/>
                <profile value="http://hl7.org/fhir/cda/ccda/StructureDefinition/2.16.840.1.113883.10.20.22.4.54"/>
            </type>
        </element>
        <element id="Supply.performer">
            <path value="Supply.performer"/>
            <short value="performer"/>
            <definition value="MAY contain zero or one [0..1] performer (CONF:1098-7461)."/>
            <min value="0"/>
            <max value="1"/>
        </element>
        <element id="Supply.performer.assignedEntity">
            <path value="Supply.performer.assignedEntity"/>
            <short value="assignedEntity"/>
            <definition value="The performer, if present, SHALL contain exactly one [1..1] assignedEntity (CONF:1098-7467)."/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Supply.performer.assignedEntity.addr">
            <path value="Supply.performer.assignedEntity.addr"/>
            <short value="addr"/>
            <definition value="This assignedEntity SHOULD contain zero or one [0..1] US Realm Address (AD.US.FIELDED) (identifier: urn:oid:2.16.840.1.113883.10.20.22.5.2) (CONF:1098-7468)."/>
            <min value="0"/>
            <max value="1"/>
            <type>
                <code value="http://hl7.org/fhir/cda/StructureDefinition/AD"/>
                <profile value="http://hl7.org/fhir/cda/ccda/StructureDefinition/2.16.840.1.113883.10.20.22.5.2"/>
            </type>
            <constraint>
                <key value="1098-10565"/>
                <severity value="error"/>
                <human value="The content of addr **SHALL** be a conformant US Realm Address (AD.US.FIELDED) (2.16.840.1.113883.10.20.22.5.2) (CONF:1098-10565)."/>
            </constraint>
        </element>
        <element id="Supply.entryRelationship">
            <path value="Supply.entryRelationship"/>
            <slicing>
                <discriminator>
                    <type value="value"/>
                    <path value="supply"/>
                </discriminator>
                <discriminator>
                    <type value="value"/>
                    <path value="typeCode"/>
                </discriminator>
                <rules value="open"/>
            </slicing>
            <definition value="MAY contain zero or one [0..1] entryRelationship (CONF:1098-7473) such that it"/>
        </element>
        <element id="Supply.entryRelationship:entryRelationship1">
            <path value="Supply.entryRelationship"/>
            <sliceName value="entryRelationship1"/>
            <short value="entryRelationship"/>
            <definition value="MAY contain zero or one [0..1] entryRelationship (CONF:1098-7473) such that it"/>
            <min value="0"/>
            <max value="1"/>
        </element>
        <element id="Supply.entryRelationship:entryRelationship1.typeCode">
            <path value="Supply.entryRelationship.typeCode"/>
            <short value="typeCode"/>
            <definition value="SHALL contain exactly one [1..1] @typeCode=&quot;REFR&quot; (CodeSystem: HL7ActRelationshipType urn:oid:2.16.840.1.113883.5.1002 STATIC) (CONF:1098-7474)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="REFR"/>
        </element>
        <element id="Supply.entryRelationship:entryRelationship1.supply">
            <path value="Supply.entryRelationship.supply"/>
            <short value="supply"/>
            <definition value="SHALL contain exactly one [1..1] Medication Supply Order (V2) (identifier: urn:hl7ii:2.16.840.1.113883.10.20.22.4.17:2014-06-09) (CONF:1098-15606)."/>
            <min value="1"/>
            <max value="1"/>
            <type>
                <code value="http://hl7.org/fhir/cda/StructureDefinition/Supply"/>
                <profile value="http://hl7.org/fhir/cda/ccda/StructureDefinition/2.16.840.1.113883.10.20.22.4.17"/>
            </type>
        </element>
    </differential>
</StructureDefinition>