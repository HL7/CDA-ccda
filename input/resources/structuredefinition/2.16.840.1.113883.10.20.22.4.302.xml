            <short value="Code for &quot;Device Catalog Number&quot;"/>
            <definition value="SHALL contain exactly one [1..1] code (CONF:4437-3433)."/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Observation.code.code">
            <path value="Observation.code.code"/>
            <short value="code"/>
            <definition value="This code SHALL contain exactly one [1..1] @code=&quot;C99286&quot; Catalog Number (CodeSystem: NCI Thesaurus (NCIt) urn:oid:2.16.840.1.113883.3.26.1.1 STATIC) (CONF:4437-3436)."/>
