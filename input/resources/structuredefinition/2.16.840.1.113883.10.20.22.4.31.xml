<?xml version="1.0" encoding="UTF-8"?>
<StructureDefinition
    xmlns="http://hl7.org/fhir">
    <id value="2.16.840.1.113883.10.20.22.4.31"/>
    <url value="http://hl7.org/cda/us/ccda/StructureDefinition/2.16.840.1.113883.10.20.22.4.31"/>
    <identifier>
        <value value="urn:oid:2.16.840.1.113883.10.20.22.4.31"/>
    </identifier>
    <name value="AgeObservation"/>
    <title value="Age Observation"/>
    <status value="active"/>
    <contact>
        <name value="Gaye Dolin"/>
        <telecom>
            <system value="phone"/>
            <value value="714-744-4152"/>
