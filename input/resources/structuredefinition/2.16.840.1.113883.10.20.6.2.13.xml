<?xml version="1.0" encoding="UTF-8"?>
<StructureDefinition
    xmlns="http://hl7.org/fhir">
    <id value="2.16.840.1.113883.10.20.6.2.13"/>
    <url value="http://hl7.org/fhir/cda/ccda/StructureDefinition/2.16.840.1.113883.10.20.6.2.13"/>
    <identifier>
        <value value="urn:oid:2.16.840.1.113883.10.20.6.2.13"/>
    </identifier>
    <name value="CodeObservations"/>
    <title value="Code Observations"/>
    <status value="active"/>
    <contact>
        <name value="Gaye Dolin"/>
        <telecom>
