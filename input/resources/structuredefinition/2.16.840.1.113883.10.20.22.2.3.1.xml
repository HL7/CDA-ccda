            </slicing>
            <min value="1"/>
        </element>
        <element id="Section.templateId:secondary">
            <path value="Section.templateId"/>
            <sliceName value="secondary"/>
            <requirements value="SHALL contain exactly one [1..1] templateId (CONF:1198-7108) such that it"/>
