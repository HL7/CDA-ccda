<?xml version="1.0" encoding="UTF-8"?>
<StructureDefinition
    xmlns="http://hl7.org/fhir">
    <id value="2.16.840.1.113883.10.20.22.4.63"/>
    <url value="http://hl7.org/fhir/cda/ccda/StructureDefinition/2.16.840.1.113883.10.20.22.4.63"/>
    <identifier>
        <value value="urn:oid:2.16.840.1.113883.10.20.22.4.63"/>
    </identifier>
    <name value="SeriesAct"/>
    <title value="Series Act"/>
    <status value="active"/>
    <contact>
        <name value="Gaye Dolin"/>
        <telecom>
            <system value="phone"/>
            <value value="714-744-4152"/>
            <use value="work"/>
        </telecom>
        <telecom>
            <system value="email"/>
            <value value="gaye.dolin@lantanagroup.com"/>
            <use value="work"/>
        </telecom>
    </contact>
    <description value="A Series Act contains the DICOM series information for referenced DICOM composite objects. The series information defines the attributes that are used to group composite instances into distinct logical sets. Each series is associated with exactly one study. Series Act clinical statements are only instantiated in the DICOM Object Catalog section inside a Study Act, and thus do not require a separate templateId; in other sections, the SOP Instance Observation is included directly."/>
    <kind value="resource"/>
    <abstract value="false"/>
    <type value="Act"/>
    <baseDefinition value="http://hl7.org/fhir/cda/StructureDefinition/Act"/>
    <derivation value="constraint"/>
    <differential>
        <element id="Act">
            <path value="Act"/>
        </element>
        <element id="Act.classCode">
            <path value="Act.classCode"/>
            <definition value="SHALL contain exactly one [1..1] @classCode=&quot;ACT&quot; Act (CodeSystem: HL7ActClass urn:oid:2.16.840.1.113883.5.6 STATIC) (CONF:81-9222)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="ACT"/>
        </element>
        <element id="Act.moodCode">
            <path value="Act.moodCode"/>
            <definition value="SHALL contain exactly one [1..1] @moodCode=&quot;EVN&quot; Event (CodeSystem: HL7ActMood urn:oid:2.16.840.1.113883.5.1001 STATIC) (CONF:81-9223)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="EVN"/>
        </element>
        <element id="Act.templateId">
            <path value="Act.templateId"/>
            <slicing>
                <discriminator>
                    <type value="value"/>
                    <path value="root"/>
                </discriminator>
                <rules value="open"/>
            </slicing>
            <definition value="SHALL contain exactly one [1..1] templateId (CONF:81-10918) such that it"/>
        </element>
        <element id="Act.templateId:primary">
            <path value="Act.templateId"/>
            <sliceName value="primary"/>
            <definition value="SHALL contain exactly one [1..1] templateId (CONF:81-10918) such that it"/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Act.templateId:primary.root">
            <path value="Act.templateId.root"/>
            <definition value="SHALL contain exactly one [1..1] @root=&quot;2.16.840.1.113883.10.20.22.4.63&quot; (CONF:81-10919)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="2.16.840.1.113883.10.20.22.4.63"/>
        </element>
        <element id="Act.id">
            <path value="Act.id"/>
            <definition value="SHALL contain at least one [1..*] id (CONF:81-9224)."/>
            <min value="1"/>
            <max value="*"/>
        </element>
        <element id="Act.id.root">
            <path value="Act.id.root"/>
            <definition value="Such ids SHALL contain exactly one [1..1] @root (CONF:81-9225)."/>
            <requirements value="The @root contains the OID of the study instance UID since DICOM study ids consist only of an OID"/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Act.id.extension">
            <path value="Act.id.extension"/>
            <definition value="Such ids SHALL NOT contain [0..0] @extension (CONF:81-9226)."/>
            <min value="0"/>
            <max value="0"/>
        </element>
        <element id="Act.code">
            <path value="Act.code"/>
            <definition value="SHALL contain exactly one [1..1] code (CONF:81-19166)."/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Act.code.code">
            <path value="Act.code.code"/>
            <definition value="This code SHALL contain exactly one [1..1] @code=&quot;113015&quot; (CONF:81-19167)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="113015"/>
        </element>
        <element id="Act.code.codeSystem">
            <path value="Act.code.codeSystem"/>
            <definition value="This code MAY contain zero or one [0..1] @codeSystem=&quot;1.2.840.10008.2.16.4&quot; (CodeSystem: DCM urn:oid:1.2.840.10008.2.16.4) (CONF:81-26461)."/>
            <min value="0"/>
            <max value="1"/>
            <patternString value="1.2.840.10008.2.16.4"/>
        </element>
        <element id="Act.code.qualifier">
            <path value="Act.code.qualifier"/>
            <definition value="This code SHALL contain exactly one [1..1] qualifier (CONF:81-26462)."/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Act.code.qualifier.name">
            <path value="Act.code.qualifier.name"/>
            <definition value="This qualifier SHALL contain exactly one [1..1] name (CONF:81-26463)."/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Act.code.qualifier.name.code">
            <path value="Act.code.qualifier.name.code"/>
            <definition value="This name SHALL contain exactly one [1..1] @code=&quot;121139&quot; Modality (CONF:81-26464)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="121139"/>
        </element>
        <element id="Act.code.qualifier.name.codeSystem">
            <path value="Act.code.qualifier.name.codeSystem"/>
            <definition value="This name SHALL contain exactly one [1..1] @codeSystem=&quot;1.2.840.10008.2.16.4&quot; (CodeSystem: DCM urn:oid:1.2.840.10008.2.16.4) (CONF:81-26465)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="1.2.840.10008.2.16.4"/>
        </element>
        <element id="Act.code.qualifier.value">
            <path value="Act.code.qualifier.value"/>
            <definition value="This qualifier SHALL contain exactly one [1..1] value (CONF:81-26466)."/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Act.text">
            <path value="Act.text"/>
            <definition value="MAY contain zero or one [0..1] text (CONF:81-9233)."/>
            <requirements value="If present, the text element contains the description of the series"/>
            <min value="0"/>
            <max value="1"/>
        </element>
        <element id="Act.effectiveTime">
            <path value="Act.effectiveTime"/>
            <definition value="SHOULD contain zero or one [0..1] effectiveTime (CONF:81-9235)."/>
            <requirements value="If present, the effectiveTime contains the time the series was started"/>
            <min value="0"/>
            <max value="1"/>
        </element>
        <element id="Act.entryRelationship">
            <path value="Act.entryRelationship"/>
            <slicing>
                <discriminator>
                    <type value="value"/>
                    <path value="observation"/>
                </discriminator>
                <discriminator>
                    <type value="value"/>
                    <path value="typeCode"/>
                </discriminator>
                <rules value="open"/>
            </slicing>
            <definition value="SHALL contain at least one [1..*] entryRelationship (CONF:81-9237) such that it"/>
        </element>
        <element id="Act.entryRelationship:entryRelationship1">
            <path value="Act.entryRelationship"/>
            <sliceName value="entryRelationship1"/>
            <definition value="SHALL contain at least one [1..*] entryRelationship (CONF:81-9237) such that it"/>
            <min value="1"/>
            <max value="*"/>
        </element>
        <element id="Act.entryRelationship:entryRelationship1.typeCode">
            <path value="Act.entryRelationship.typeCode"/>
            <definition value="SHALL contain exactly one [1..1] @typeCode=&quot;COMP&quot; Component (CodeSystem: HL7ActRelationshipType urn:oid:2.16.840.1.113883.5.1002 STATIC) (CONF:81-9238)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="COMP"/>
        </element>
        <element id="Act.entryRelationship:entryRelationship1.observation">
            <path value="Act.entryRelationship.observation"/>
            <definition value="SHALL contain exactly one [1..1] observationSOP Instance Observation (identifier: urn:oid:2.16.840.1.113883.10.20.6.2.8) (CONF:81-15927)."/>
            <min value="1"/>
            <max value="1"/>
            <type>
                <code value="http://hl7.org/fhir/cda/StructureDefinition/Observation"/>
                <profile value="http://hl7.org/fhir/cda/ccda/StructureDefinition/2.16.840.1.113883.10.20.6.2.8"/>
            </type>
        </element>
    </differential>
</StructureDefinition>