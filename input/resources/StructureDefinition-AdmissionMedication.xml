<?xml version="1.0" encoding="UTF-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
    <id value="AdmissionMedication"/>
    <extension url="http://hl7.org/fhir/tools/StructureDefinition/logical-target">
        <valueBoolean>
            <extension url="http://hl7.org/fhir/StructureDefinition/data-absent-reason">
                <valueCode value="not-applicable"/>
            </extension>
        </valueBoolean>
    </extension>
    <extension url="http://hl7.org/fhir/tools/StructureDefinition/logical-container">
        <valueUri value="http://hl7.org/cda/stds/core/StructureDefinition/ClinicalDocument"/>
    </extension>
    <extension url="http://hl7.org/fhir/tools/StructureDefinition/xml-namespace">
        <valueUri value="urn:hl7-org:v3"/>
    </extension>
    <extension url="http://hl7.org/fhir/tools/StructureDefinition/xml-name">
        <valueString value="act"/>
    </extension>
    <extension url="http://hl7.org/fhir/tools/StructureDefinition/type-profile-style">
        <valueUri value="cda"/>
    </extension>
    <url value="http://hl7.org/cda/us/ccda/StructureDefinition/AdmissionMedication"/>
    <identifier>
        <value value="urn:hl7ii:2.16.840.1.113883.10.20.22.4.36:2014-06-09"/>
    </identifier>
    <version value="2014-06-09"/>
    <name value="AdmissionMedication"/>
    <title value="Admission Medication"/>
    <status value="draft"/>
    <description value="This template represents the medications taken by the patient prior to and at the time of admission."/>
    <fhirVersion value="5.0.0"/>
    <mapping>
        <identity value="rim"/>
        <uri value="http://hl7.org/v3"/>
        <name value="RIM Mapping"/>
    </mapping>
    <kind value="logical"/>
    <abstract value="false"/>
    <type value="http://hl7.org/cda/stds/core/StructureDefinition/Act"/>
    <baseDefinition value="http://hl7.org/cda/stds/core/StructureDefinition/Act"/>
    <derivation value="constraint"/>
    <differential>
        <element id="Act">
            <path value="Act"/>
            <constraint>
                <key value="should-text-ref-value"/>
                <severity value="warning"/>
                <human value="SHOULD contain text/reference/@value"/>
                <expression value="text.reference.value.exists()"/>
                <source value="http://hl7.org/cda/us/ccda/StructureDefinition/AdmissionMedication"/>
            </constraint>
        </element>
        <element id="Act.templateId">
            <path value="Act.templateId"/>
            <slicing>
                <discriminator>
                    <type value="value"/>
                    <path value="root"/>
                </discriminator>
                <discriminator>
                    <type value="value"/>
                    <path value="extension"/>
                </discriminator>
                <rules value="open"/>
            </slicing>
            <min value="1"/>
        </element>
        <element id="Act.templateId:admission-med">
            <path value="Act.templateId"/>
            <sliceName value="admission-med"/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Act.templateId:admission-med.root">
            <path value="Act.templateId.root"/>
            <min value="1"/>
            <patternString value="2.16.840.1.113883.10.20.22.4.36"/>
        </element>
        <element id="Act.templateId:admission-med.extension">
            <path value="Act.templateId.extension"/>
            <min value="1"/>
            <patternString value="2014-06-09"/>
        </element>
        <element id="Act.classCode">
            <path value="Act.classCode"/>
            <comment value="SHALL contain exactly one [1..1] @classCode=&quot;ACT&quot; (CodeSystem: HL7ActClass urn:oid:2.16.840.1.113883.5.6 STATIC) (CONF:1098-7698)."/>
            <fixedCode value="ACT"/>
        </element>
        <element id="Act.moodCode">
            <path value="Act.moodCode"/>
            <comment value="SHALL contain exactly one [1..1] @moodCode=&quot;EVN&quot; (CodeSystem: HL7ActMood urn:oid:2.16.840.1.113883.5.1001 STATIC) (CONF:1098-7699)."/>
            <fixedCode value="EVN"/>
        </element>
        <element id="Act.code">
            <path value="Act.code"/>
            <short value="Medications on admission (narrative)"/>
            <comment value="This code SHALL contain exactly one [1..1] @code=&quot;42346-7&quot; Medications on Admission (CONF:1098-15519)."/>
        </element>
        <element id="Act.code.code">
            <path value="Act.code.code"/>
            <min value="1"/>
            <patternCode value="42346-7"/>
        </element>
        <element id="Act.code.codeSystem">
            <path value="Act.code.codeSystem"/>
            <short value="LOINC"/>
            <min value="1"/>
            <patternString value="2.16.840.1.113883.6.1"/>
        </element>
        <element id="Act.text">
            <path value="Act.text"/>
            <short value="SHOULD reference the portion of section narrative text corresponding to this entry"/>
        </element>
        <element id="Act.text.reference">
            <path value="Act.text.reference"/>
            <constraint>
                <key value="value-starts-octothorpe"/>
                <severity value="error"/>
                <human value="If reference/@value is present, it SHALL begin with a '#' and SHALL point to its corresponding narrative"/>
                <expression value="value.exists() implies value.startsWith('#')"/>
                <source value="http://hl7.org/cda/us/ccda/StructureDefinition/AdmissionMedication"/>
            </constraint>
        </element>
        <element id="Act.entryRelationship">
            <path value="Act.entryRelationship"/>
            <slicing>
                <discriminator>
                    <type value="profile"/>
                    <path value="substanceAdministration"/>
                </discriminator>
                <rules value="open"/>
            </slicing>
            <min value="1"/>
        </element>
        <element id="Act.entryRelationship:medicationActivity">
            <path value="Act.entryRelationship"/>
            <sliceName value="medicationActivity"/>
            <comment value="SHALL contain at least one [1..*] entryRelationship (CONF:1098-7701) such that it"/>
            <min value="1"/>
            <max value="*"/>
        </element>
        <element id="Act.entryRelationship:medicationActivity.typeCode">
            <path value="Act.entryRelationship.typeCode"/>
            <comment value="SHALL contain exactly one [1..1] @typeCode=&quot;SUBJ&quot; (CodeSystem: HL7ActRelationshipType urn:oid:2.16.840.1.113883.5.1002 STATIC) (CONF:1098-7702)."/>
            <fixedCode value="SUBJ"/>
        </element>
        <element id="Act.entryRelationship:medicationActivity.substanceAdministration">
            <path value="Act.entryRelationship.substanceAdministration"/>
            <comment value="SHALL contain exactly one [1..1] Medication Activity (identifier: urn:hl7ii:2.16.840.1.113883.10.20.22.4.16:2014-06-09) (CONF:1098-15520)."/>
            <min value="1"/>
            <type>
                <code value="http://hl7.org/cda/stds/core/StructureDefinition/SubstanceAdministration"/>
                <profile value="http://hl7.org/cda/us/ccda/StructureDefinition/MedicationActivity"/>
            </type>
        </element>
    </differential>
</StructureDefinition>