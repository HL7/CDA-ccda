    </extension>
    <extension url="http://hl7.org/fhir/tools/StructureDefinition/logical-container">
        <valueUri value="http://hl7.org/cda/stds/core/StructureDefinition/ClinicalDocument"/>
    </extension>
    <extension url="http://hl7.org/fhir/tools/StructureDefinition/type-profile-style">
        <valueCode value="cda"/>
    </extension>
    <url value="http://hl7.org/cda/us/ccda/StructureDefinition/AgeRangeObservation"/>
    <identifier>
        <system value="urn:ietf:rfc:3986"/>
        <value value="urn:hl7ii:2.16.840.1.113883.10.20.22.4.516:2025-05-01"/>
    </identifier>
    <version value="2025-05-01"/>
    <name value="AgeRangeObservation"/>
    <title value="Age Range Observation"/>
    <status value="draft"/>
    <description value="This Age Range Observation captures an estimated age range when the subject's exact age is unknown. It is used in cases where only an approximate age, expressed as a range within specific age units (e.g., years), is available. For example, a person might report, &quot;My father was between 50 and 55 years old when he developed Parkinson's disease.&quot; "/>
    <fhirVersion value="5.0.0"/>
    <kind value="logical"/>
    <abstract value="false"/>
    <type value="http://hl7.org/cda/stds/core/StructureDefinition/Observation"/>
    <baseDefinition value="http://hl7.org/cda/stds/core/StructureDefinition/Observation"/>
    <derivation value="constraint"/>
    <differential>
        <element id="Observation">
            <path value="Observation"/>
            <constraint>
                <key value="should-text-ref-value"/>
                <severity value="warning"/>
                <human value="SHOULD contain text/reference/@value"/>
                <expression value="text.reference.value.exists()"/>
                <source value="http://hl7.org/cda/us/ccda/StructureDefinition/AgeRangeObservation"/>
            </constraint>
        </element>
        <element id="Observation.templateId">
            <path value="Observation.templateId"/>
            <slicing>
                <discriminator>
                    <type value="value"/>
                    <path value="root"/>
                </discriminator>
                <discriminator>
                    <type value="value"/>
                    <path value="extension"/>
                </discriminator>
                <rules value="open"/>
            </slicing>
            <min value="1"/>
        </element>
        <element id="Observation.templateId:age-range-obs">
            <path value="Observation.templateId"/>
            <sliceName value="age-range-obs"/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Observation.templateId:age-range-obs.root">
            <path value="Observation.templateId.root"/>
            <min value="1"/>
            <patternString value="2.16.840.1.113883.10.20.22.4.516"/>
        </element>
        <element id="Observation.templateId:age-range-obs.extension">
            <path value="Observation.templateId.extension"/>
            <min value="1"/>
            <patternString value="2025-05-01"/>
        </element>
        <element id="Observation.classCode">
            <path value="Observation.classCode"/>
            <comment value="SHALL contain exactly one [1..1] @classCode=&quot;OBS&quot; Observation (CodeSystem: HL7ActClass urn:oid:2.16.840.1.113883.5.6 STATIC) (CONF:81-7613)."/>
            <fixedCode value="OBS"/>
        </element>
        <element id="Observation.moodCode">
            <path value="Observation.moodCode"/>
            <comment value="SHALL contain exactly one [1..1] @moodCode=&quot;EVN&quot; Event (CodeSystem: HL7ActMood urn:oid:2.16.840.1.113883.5.1001 STATIC) (CONF:81-7614)."/>
            <fixedCode value="EVN"/>
        </element>
        <element id="Observation.code">
            <path value="Observation.code"/>
            <short value="Age at onset of clinical finding"/>
            <comment value="This code SHALL contain exactly one [1..1] @code=&quot;445518008&quot; Age At Onset (CONF:81-16776)."/>
        </element>
        <element id="Observation.code.code">
            <path value="Observation.code.code"/>
            <min value="1"/>
            <patternCode value="445518008"/>
        </element>
        <element id="Observation.code.codeSystem">
            <path value="Observation.code.codeSystem"/>
            <short value="SNOMED CT"/>
            <min value="1"/>
            <patternString value="2.16.840.1.113883.6.96"/>
        </element>
        <element id="Observation.text">
            <path value="Observation.text"/>
            <short value="SHOULD reference the portion of section narrative text corresponding to this entry"/>
            <condition value="should-text-ref-value"/>
        </element>
        <element id="Observation.text.reference">
            <path value="Observation.text.reference"/>
            <constraint>
                <key value="value-starts-octothorpe"/>
                <severity value="error"/>
                <human value="If reference/@value is present, it SHALL begin with a '#' and SHALL point to its corresponding narrative"/>
                <expression value="value.exists() implies value.startsWith('#')"/>
                <source value="http://hl7.org/cda/us/ccda/StructureDefinition/AgeRangeObservation"/>
            </constraint>
        </element>
        <element id="Observation.statusCode">
            <path value="Observation.statusCode"/>
            <comment value="SHALL contain exactly one [1..1] statusCode (CONF:81-15965)."/>
            <min value="1"/>
        </element>
        <element id="Observation.statusCode.code">
            <path value="Observation.statusCode.code"/>
            <comment value="This statusCode SHALL contain exactly one [1..1] @code=&quot;completed&quot; Completed (CodeSystem: HL7ActStatus urn:oid:2.16.840.1.113883.5.14 STATIC) (CONF:81-15966)."/>
            <min value="1"/>
            <fixedCode value="completed"/>
        </element>
        <element id="Observation.value">
            <path value="Observation.value"/>
            <slicing>
                <discriminator>
                    <type value="type"/>
                    <path value="$this"/>
                </discriminator>
                <rules value="open"/>
            </slicing>
            <short value="Represents the age range during which the event or observation occurred"/>
            <min value="1"/>
            <max value="1"/>
            <type>
                <code value="http://hl7.org/cda/stds/core/StructureDefinition/ST"/>
            </type>
            <type>
                <code value="http://hl7.org/cda/stds/core/StructureDefinition/IVL-PQ"/>
            </type>
        </element>
        <element id="Observation.value:range">
            <path value="Observation.value"/>
            <sliceName value="range"/>
            <short value="Indicates the age range during which the event or observation occurred. Use when actual ages are known."/>
            <min value="0"/>
            <max value="1"/>
            <type>
                <code value="http://hl7.org/cda/stds/core/StructureDefinition/IVL-PQ"/>
            </type>
        </element>
        <element id="Observation.value:range.low">
            <path value="Observation.value.low"/>
            <comment value="This value SHALL contain exactly one [1..1] low."/>
            <min value="1"/>
        </element>
        <element id="Observation.value:range.low.unit">
            <path value="Observation.value.low.unit"/>
            <min value="1"/>
            <binding>
                <strength value="required"/>
                <valueSet value="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.11.20.9.21"/>
            </binding>
        </element>
        <element id="Observation.value:range.high">
            <path value="Observation.value.high"/>
            <comment value="This value MAY contain exactly one [0..1] high."/>
        </element>
        <element id="Observation.value:range.high.unit">
            <path value="Observation.value.high.unit"/>
            <min value="1"/>
            <binding>
                <strength value="required"/>
                <valueSet value="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.11.20.9.21"/>
            </binding>
        </element>
        <element id="Observation.value:text">
            <path value="Observation.value"/>
            <sliceName value="text"/>
            <short value="A human-readable description of the age range."/>
            <min value="0"/>
            <max value="1"/>
            <type>
                <code value="http://hl7.org/cda/stds/core/StructureDefinition/ST"/>
            </type>
        </element>
    </differential>
</StructureDefinition>