<?xml version="1.0" encoding="UTF-8"?>
<StructureDefinition
    xmlns="http://hl7.org/fhir">
    <id value="NutritionAssessment"/>
    <extension url="http://hl7.org/fhir/tools/StructureDefinition/logical-target">
      <valueBoolean>
        <extension url="http://hl7.org/fhir/StructureDefinition/data-absent-reason">
          <valueCode value="not-applicable"/>
        </extension>
      </valueBoolean>
    </extension>
    <url value="http://hl7.org/cda/us/ccda/StructureDefinition/NutritionAssessment"/>
    <identifier>
        <value value="urn:oid:2.16.840.1.113883.10.20.22.4.138"/>
    </identifier>
    <name value="NutritionAssessment"/>
    <title value="Nutrition Assessment"/>
    <status value="active"/>
    <description value="This template represents the patient's nutrition abilities and habits including intake, diet requirements or diet followed."/>
    <fhirVersion value="4.0.1"/>
    <kind value="logical"/>
    <abstract value="false"/>
    <type value="http://hl7.org/cda/stds/core/StructureDefinition/Observation"/>
    <baseDefinition value="http://hl7.org/cda/stds/core/StructureDefinition/Observation"/>
    <derivation value="constraint"/>
    <differential>
        <element id="Observation">
            <path value="Observation"/>
        </element>
        <element id="Observation.classCode">
            <path value="Observation.classCode"/>
            <comment value="SHALL contain exactly one [1..1] @classCode=&quot;OBS&quot; Observation (CodeSystem: HL7ActClass urn:oid:2.16.840.1.113883.5.6 STATIC) (CONF:1098-32914)."/>
            <min value="1"/>
            <max value="1"/>
            <fixedCode value="OBS"/>
        </element>
        <element id="Observation.moodCode">
            <path value="Observation.moodCode"/>
            <comment value="SHALL contain exactly one [1..1] @moodCode=&quot;EVN&quot; Event (CodeSystem: HL7ActMood urn:oid:2.16.840.1.113883.5.1001 STATIC) (CONF:1098-32915)."/>
            <min value="1"/>
            <max value="1"/>
            <fixedCode value="EVN"/>
        </element>
        <element id="Observation.templateId">
            <path value="Observation.templateId"/>
            <slicing>
                <discriminator>
                    <type value="value"/>
                    <path value="root"/>
                </discriminator>
                <rules value="open"/>
            </slicing>
            <min value="1"/>
        </element>
        <element id="Observation.templateId:primary">
            <path value="Observation.templateId"/>
            <sliceName value="primary"/>
            <comment value="SHALL contain exactly one [1..1] templateId (CONF:1098-32916) such that it, SHALL not contain [0..0] extension."/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Observation.templateId:primary.root">
            <path value="Observation.templateId.root"/>
            <comment value="SHALL contain exactly one [1..1] @root=&quot;2.16.840.1.113883.10.20.22.4.138&quot; (CONF:1098-32917)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="2.16.840.1.113883.10.20.22.4.138"/>
        </element>
        <element id="Observation.templateId:primary.extension">
            <path value="Observation.templateId.extension"/>
            <min value="0"/>
            <max value="0"/>
        </element>
        <element id="Observation.id">
            <path value="Observation.id"/>
            <comment value="SHALL contain at least one [1..*] id (CONF:1098-32918)."/>
            <min value="1"/>
            <max value="*"/>
        </element>
        <element id="Observation.code">
            <path value="Observation.code"/>
            <comment value="SHALL contain exactly one [1..1] code (CONF:1098-32919)."/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Observation.code.code">
            <path value="Observation.code.code"/>
            <comment value="This code SHALL contain exactly one [1..1] @code=&quot;75303-8&quot; Nutrition assessment (CONF:1098-32926)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="75303-8"/>
        </element>
        <element id="Observation.code.codeSystem">
            <path value="Observation.code.codeSystem"/>
            <comment value="This code SHALL contain exactly one [1..1] @codeSystem=&quot;2.16.840.1.113883.6.1&quot; (CodeSystem: LOINC urn:oid:2.16.840.1.113883.6.1) (CONF:1098-32927)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="2.16.840.1.113883.6.1"/>
        </element>
        <element id="Observation.statusCode">
            <path value="Observation.statusCode"/>
            <comment value="SHALL contain exactly one [1..1] statusCode (CONF:1098-32920)."/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Observation.statusCode.code">
            <path value="Observation.statusCode.code"/>
            <comment value="This statusCode SHALL contain exactly one [1..1] @code=&quot;completed&quot; Completed (CodeSystem: HL7ActStatus urn:oid:2.16.840.1.113883.5.14 STATIC) (CONF:1098-32921)."/>
            <min value="1"/>
            <max value="1"/>
            <fixedString value="completed"/>
        </element>
        <element id="Observation.effectiveTime">
            <path value="Observation.effectiveTime"/>
            <comment value="SHALL contain exactly one [1..1] effectiveTime (CONF:1098-32923)."/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Observation.value">
            <path value="Observation.value"/>
            <comment value="SHALL contain exactly one [1..1] value (CONF:1098-32922)."/>
            <min value="1"/>
            <max value="1"/>
            <constraint>
                <key value="1098-32925"/>
                <severity value="warning"/>
                <human value="If xsi:type=“CD”, **SHOULD** contain a code from SNOMED CT (CodeSystem: 2.16.840.1.113883.6.96) (CONF:1098-32925)."/>
            </constraint>
        </element>
        <element id="Observation.author">
            <path value="Observation.author"/>
            <comment value="SHOULD contain zero or more [0..*] Author Participation (identifier: urn:oid:2.16.840.1.113883.10.20.22.4.119) (CONF:1098-32924)."/>
            <min value="0"/>
            <max value="*"/>
            <type>
                <code value="http://hl7.org/cda/stds/core/StructureDefinition/Author"/>
                <profile value="http://hl7.org/cda/us/ccda/StructureDefinition/AuthorParticipation"/>
            </type>
        </element>
    </differential>
</StructureDefinition>