            </constraint>
        </element>
        <element id="Observation.effectiveTime.value">
            <path value="Observation.effectiveTime.value"/>
            <short value="value should be used instead of low/high"/>
        </element>
        <element id="Observation.value">
            <path value="Observation.value"/>
            <min value="1"/>
            <max value="1"/>
            <type>
