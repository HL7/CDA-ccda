<StructureDefinition xmlns="http://hl7.org/fhir">
  <id value="E_Medical_Equipment_Organizer" />
  <url value="urn:oid:2.16.840.1.113883.10.20.22.4.135" />
  <name value="Medical Equipment Organizer" />
  <status value="active" />
  <contact>
    <name value="Gaye Dolin" />
    <telecom>
      <system value="phone" />
      <value value="714-744-4152" />
      <use value="work" />
    </telecom>
    <telecom>
      <system value="email" />
      <value value="gaye.dolin@lantanagroup.com" />
