            <min value="1"/>
            <max value="1"/>
            <patternString value="EVN"/>
        </element>
        <element id="Observation.templateId">
            <path value="Observation.templateId"/>
            <slicing>
                <discriminator>
                    <type value="value"/>
                    <path value="root"/>
                </discriminator>
                <rules value="open"/>
            </slicing>
            <definition value="SHALL contain exactly one [1..1] templateId (CONF:81-9290) such that it"/>
        </element>
        <element id="Observation.templateId:templateId1">
