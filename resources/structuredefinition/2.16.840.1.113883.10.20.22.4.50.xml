<?xml version="1.0" encoding="UTF-8"?>
<StructureDefinition
    xmlns="http://hl7.org/fhir">
    <id value="2.16.840.1.113883.10.20.22.4.50"/>
    <url value="http://hl7.org/cda/us/ccda/StructureDefinition/2.16.840.1.113883.10.20.22.4.50"/>
    <identifier>
        <value value="urn:hl7ii:2.16.840.1.113883.10.20.22.4.50:2014-06-09"/>
    </identifier>
    <version value="2014-06-09"/>
    <name value="Non-Medicinal Supply Activity (V2)"/>
    <status value="active"/>
    <contact>
        <name value="Gaye Dolin"/>
        <telecom>
            <system value="phone"/>
            <value value="714-744-4152"/>
            <use value="work"/>
        </telecom>
        <telecom>
            <system value="email"/>
            <value value="gaye.dolin@lantanagroup.com"/>
            <use value="work"/>
