    </contact>
    <description value="This template represents equipment supplied to the patient (e.g., pumps, inhalers, wheelchairs). Devices applied to, or placed in, the patient are represented with the Product Instance entry contained within a Procedure Activity Procedure (V2) (identifier: urn:hl7ii: 2.16.840.1.113883.10.20.22.4.14)"/>
    <kind value="resource"/>
    <abstract value="false"/>
    <context/>
    <type value="Supply"/>
    <baseDefinition value="http://hl7.org/fhir/cda/StructureDefinition/Supply"/>
    <derivation value="constraint"/>
    <differential>
        <element id="Supply">
            <path value="Supply"/>
        </element>
        <element id="Supply.classCode">
            <path value="Supply.classCode"/>
            <short value="classCode"/>
            <definition value="SHALL contain exactly one [1..1] @classCode=&quot;SPLY&quot; (CodeSystem: HL7ActClass urn:oid:2.16.840.1.113883.5.6 STATIC) (CONF:1098-8745)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="SPLY"/>
        </element>
        <element id="Supply.moodCode">
            <path value="Supply.moodCode"/>
            <short value="moodCode"/>
            <definition value="SHALL contain exactly one [1..1] @moodCode, which SHALL be selected from ValueSet MoodCodeEvnInt urn:oid:2.16.840.1.113883.11.20.9.18 STATIC 2011-04-03 (CONF:1098-8746)."/>
            <min value="1"/>
            <max value="1"/>
            <binding>
                <strength value="required"/>
            </binding>
        </element>
        <element id="Supply.templateId">
            <path value="Supply.templateId"/>
            <slicing>
                <discriminator>
                    <type value="value"/>
                    <path value="extension"/>
                </discriminator>
                <discriminator>
                    <type value="value"/>
                    <path value="root"/>
                </discriminator>
                <rules value="open"/>
            </slicing>
            <definition value="SHALL contain exactly one [1..1] templateId (CONF:1098-8747) such that it"/>
        </element>
        <element id="Supply.templateId:templateId1">
            <path value="Supply.templateId"/>
            <sliceName value="templateId1"/>
            <short value="templateId"/>
            <definition value="SHALL contain exactly one [1..1] templateId (CONF:1098-8747) such that it"/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Supply.templateId:templateId1.root">
            <path value="Supply.templateId.root"/>
            <sliceName value="templateId1"/>
            <short value="root"/>
            <definition value="SHALL contain exactly one [1..1] @root=&quot;2.16.840.1.113883.10.20.22.4.50&quot; (CONF:1098-10509)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="2.16.840.1.113883.10.20.22.4.50"/>
        </element>
        <element id="Supply.templateId:templateId1.extension">
            <path value="Supply.templateId.extension"/>
            <sliceName value="templateId1"/>
            <short value="extension"/>
            <definition value="SHALL contain exactly one [1..1] @extension=&quot;2014-06-09&quot; (CONF:1098-32514)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="2014-06-09"/>
        </element>
        <element id="Supply.id">
            <path value="Supply.id"/>
            <short value="id"/>
            <definition value="SHALL contain at least one [1..*] id (CONF:1098-8748)."/>
            <min value="1"/>
            <max value="*"/>
        </element>
        <element id="Supply.statusCode">
            <path value="Supply.statusCode"/>
            <short value="statusCode"/>
            <definition value="SHALL contain exactly one [1..1] statusCode (CONF:1098-8749)."/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Supply.statusCode.code">
            <path value="Supply.statusCode.code"/>
            <short value="code"/>
            <definition value="This statusCode SHALL contain exactly one [1..1] @code, which SHALL be selected from ValueSet ActStatus urn:oid:2.16.840.1.113883.1.11.15933 DYNAMIC (CONF:1098-32363)."/>
            <min value="1"/>
            <max value="1"/>
            <binding>
                <strength value="required"/>
            </binding>
        </element>
        <element id="Supply.effectiveTime">
            <path value="Supply.effectiveTime"/>
            <short value="effectiveTime"/>
            <definition value="SHOULD contain zero or one [0..1] effectiveTime (CONF:1098-15498)."/>
            <min value="0"/>
            <max value="1"/>
        </element>
        <element id="Supply.quantity">
            <path value="Supply.quantity"/>
            <short value="quantity"/>
            <definition value="SHOULD contain zero or one [0..1] quantity (CONF:1098-8751)."/>
            <min value="0"/>
            <max value="1"/>
        </element>
        <element id="Supply.participant">
            <path value="Supply.participant"/>
            <slicing>
                <discriminator>
                    <type value="value"/>
                    <path value="participantRole"/>
                </discriminator>
                <discriminator>
                    <type value="value"/>
                    <path value="typeCode"/>
                </discriminator>
                <rules value="open"/>
            </slicing>
            <definition value="MAY contain zero or one [0..1] participant (CONF:1098-8752) such that it"/>
        </element>
        <element id="Supply.participant:participant1">
            <path value="Supply.participant"/>
            <sliceName value="participant1"/>
            <short value="participant"/>
            <definition value="MAY contain zero or one [0..1] participant (CONF:1098-8752) such that it"/>
            <min value="0"/>
            <max value="1"/>
        </element>
        <element id="Supply.participant:participant1.typeCode">
            <path value="Supply.participant.typeCode"/>
            <sliceName value="participant1"/>
            <short value="typeCode"/>
            <definition value="SHALL contain exactly one [1..1] @typeCode=&quot;PRD&quot; Product (CodeSystem: HL7ParticipationType urn:oid:2.16.840.1.113883.5.90 STATIC) (CONF:1098-8754)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="PRD"/>
        </element>
        <element id="Supply.participant:participant1.participantRole">
            <path value="Supply.participant.participantRole"/>
            <sliceName value="participant1"/>
            <short value="participantRole"/>
            <definition value="SHALL contain exactly one [1..1] Product Instance (identifier: urn:oid:2.16.840.1.113883.10.20.22.4.37) (CONF:1098-15900)."/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Supply.entryRelationship">
            <path value="Supply.entryRelationship"/>
            <slicing>
                <discriminator>
                    <type value="value"/>
                    <path value="typeCode"/>
                </discriminator>
                <discriminator>
                    <type value="value"/>
                    <path value="inversionInd"/>
                </discriminator>
                <discriminator>
                    <type value="value"/>
                    <path value="act"/>
                </discriminator>
                <rules value="open"/>
            </slicing>
            <definition value="MAY contain zero or one [0..1] entryRelationship (CONF:1098-30277) such that it"/>
        </element>
        <element id="Supply.entryRelationship:entryRelationship1">
            <path value="Supply.entryRelationship"/>
            <sliceName value="entryRelationship1"/>
            <short value="entryRelationship"/>
            <definition value="MAY contain zero or one [0..1] entryRelationship (CONF:1098-30277) such that it"/>
            <min value="0"/>
            <max value="1"/>
        </element>
        <element id="Supply.entryRelationship:entryRelationship1.typeCode">
            <path value="Supply.entryRelationship.typeCode"/>
            <sliceName value="entryRelationship1"/>
            <short value="typeCode"/>
            <definition value="SHALL contain exactly one [1..1] @typeCode=&quot;SUBJ&quot; (CONF:1098-30278)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="SUBJ"/>
        </element>
        <element id="Supply.entryRelationship:entryRelationship1.inversionInd">
            <path value="Supply.entryRelationship.inversionInd"/>
            <sliceName value="entryRelationship1"/>
            <short value="inversionInd"/>
            <definition value="SHALL contain exactly one [1..1] @inversionInd=&quot;TRUE&quot; (CONF:1098-30279)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="TRUE"/>
        </element>
        <element id="Supply.entryRelationship:entryRelationship1.act">
            <path value="Supply.entryRelationship.act"/>
            <sliceName value="entryRelationship1"/>
            <short value="act"/>
            <definition value="SHALL contain exactly one [1..1] Instruction (V2) (identifier: urn:hl7ii:2.16.840.1.113883.10.20.22.4.20:2014-06-09) (CONF:1098-31393)."/>
            <min value="1"/>
            <max value="1"/>
        </element>
    </differential>
</StructureDefinition>