    <element id="Supply.effectiveTime:effectiveTime1">
      <path value="Supply.effectiveTime" />
      <sliceName value="effectiveTime1" />
      <short value="effectiveTime" />
      <definition value="SHOULD contain zero or one [0..1] effectiveTime (CONF:1098-15143) such that it" />
      <min value="0" />
      <max value="1" />
