    <element id="Supply.statusCode.code">
      <path value="Supply.statusCode.code" />
      <short value="code" />
      <definition value="This statusCode SHALL contain exactly one [1..1] @code, which SHALL be selected from ValueSet ActStatus urn:oid:2.16.840.1.113883.1.11.15933 DYNAMIC (CONF:1098-32362)." />
