<?xml version="1.0" encoding="UTF-8"?>
<StructureDefinition
    xmlns="http://hl7.org/fhir">
    <id value="2.16.840.1.113883.10.20.6.2.5"/>
    <url value="http://hl7.org/fhir/cda/ccda/StructureDefinition/2.16.840.1.113883.10.20.6.2.5"/>
    <identifier>
        <value value="urn:oid:2.16.840.1.113883.10.20.6.2.5"/>
    </identifier>
    <name value="ProcedureContext"/>
    <title value="Procedure Context"/>
    <status value="active"/>
    <contact>
        <name value="Gaye Dolin"/>
        <telecom>
            <system value="phone"/>
            <value value="714-744-4152"/>
            <use value="work"/>
        </telecom>
        <telecom>
            <system value="email"/>
            <value value="gaye.dolin@lantanagroup.com"/>
            <use value="work"/>
        </telecom>
    </contact>
    <description value="The ServiceEvent Procedure Context of the document header may be overridden in the CDA structured body if there is a need to refer to multiple imaging procedures or acts. The selection of the Procedure or Act entry from the clinical statement choice box depends on the nature of the imaging service that has been performed. The Procedure entry shall be used for image-guided interventions and minimally invasive imaging services, whereas the Act entry shall be used for diagnostic imaging services."/>
    <kind value="resource"/>
    <abstract value="false"/>
    <type value="Act"/>
    <baseDefinition value="http://hl7.org/fhir/cda/StructureDefinition/Act"/>
    <derivation value="constraint"/>
    <differential>
        <element id="Act">
            <path value="Act"/>
        </element>
        <element id="Act.classCode">
            <path value="Act.classCode"/>
            <short value="classCode"/>
            <definition value="SHALL contain exactly one [1..1] @classCode=&quot;ACT&quot; (CodeSystem: HL7ActClass urn:oid:2.16.840.1.113883.5.6) (CONF:81-26452)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="ACT"/>
        </element>
        <element id="Act.moodCode">
            <path value="Act.moodCode"/>
            <short value="moodCode"/>
            <definition value="SHALL contain exactly one [1..1] @moodCode=&quot;EVN&quot; (CodeSystem: HL7ActMood urn:oid:2.16.840.1.113883.5.1001) (CONF:81-26453)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="EVN"/>
        </element>
        <element id="Act.templateId">
            <path value="Act.templateId"/>
            <slicing>
                <discriminator>
                    <type value="value"/>
                    <path value="root"/>
                </discriminator>
                <rules value="open"/>
            </slicing>
            <definition value="SHALL contain exactly one [1..1] templateId (CONF:81-9200) such that it"/>
        </element>
        <element id="Act.templateId:templateId1">
            <path value="Act.templateId"/>
            <sliceName value="templateId1"/>
            <short value="templateId"/>
            <definition value="SHALL contain exactly one [1..1] templateId (CONF:81-9200) such that it"/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Act.templateId:templateId1.root">
            <path value="Act.templateId.root"/>
            <sliceName value="templateId1"/>
            <short value="root"/>
            <definition value="SHALL contain exactly one [1..1] @root=&quot;2.16.840.1.113883.10.20.6.2.5&quot; (CONF:81-10530)."/>
            <min value="1"/>
            <max value="1"/>
            <patternString value="2.16.840.1.113883.10.20.6.2.5"/>
        </element>
        <element id="Act.code">
            <path value="Act.code"/>
            <short value="code"/>
            <definition value="SHALL contain exactly one [1..1] code (CONF:81-9201)."/>
            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="Act.effectiveTime">
            <path value="Act.effectiveTime"/>
            <short value="effectiveTime"/>
            <definition value="SHOULD contain zero or one [0..1] effectiveTime (CONF:81-9203)."/>
            <min value="0"/>
            <max value="1"/>
        </element>
        <element id="Act.effectiveTime.value">
            <path value="Act.effectiveTime.value"/>
            <short value="value"/>
            <definition value="The effectiveTime, if present, SHALL contain exactly one [1..1] @value (CONF:81-17173)."/>
            <min value="1"/>
            <max value="1"/>
        </element>
    </differential>
</StructureDefinition>