        </element>
        <element id="AD.country">
            <path value="AD.country"/>
            <short value="country"/>
            <definition value="SHOULD contain zero or one [0..1] country, which SHALL be selected from ValueSet Country urn:oid:2.16.840.1.113883.3.88.12.80.63 DYNAMIC (CONF:81-7295)."/>
            <min value="0"/>
            <max value="1"/>
            <binding>
