            <min value="1"/>
            <max value="1"/>
        </element>
        <element id="AD.postalCode">
            <path value="AD.postalCode"/>
            <short value="postalCode"/>
            <definition value="SHOULD contain zero or one [0..1] postalCode, which SHOULD be selected from ValueSet PostalCode urn:oid:2.16.840.1.113883.3.88.12.80.2 DYNAMIC (CONF:81-7294)."/>
