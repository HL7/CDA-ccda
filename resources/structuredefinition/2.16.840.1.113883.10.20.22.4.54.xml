        <element id="ManufacturedProduct.classCode">
            <path value="ManufacturedProduct.classCode"/>
            <short value="classCode"/>
            <definition value="SHALL contain exactly one [1..1] @classCode=&quot;MANU&quot; (CodeSystem: HL7RoleClass urn:oid:2.16.840.1.113883.5.110 STATIC) (CONF:1098-9002)."/>
